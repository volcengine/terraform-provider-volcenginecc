{
  "additionalProperties": false,
  "createOnlyProperties": [
    "/properties/Kind",
    "/properties/ZoneId",
    "/properties/SourceSnapshotId",
    "/properties/ProjectName",
    "/properties/PayType"
  ],
  "definitions": {
    "BaselinePerformance": {
      "description": "基本性能",
      "properties": {
        "IOPS": {
          "description": "云盘的总IOPS，即云盘的基准IOPS和额外IOPS之和。",
          "type": "number"
        },
        "Throughput": {
          "description": "云盘的总吞吐量，即云盘的基准吞吐量和额外吞吐量之和。",
          "type": "number"
        }
      },
      "type": "object"
    },
    "Performance": {
      "description": "额外性能",
      "properties": {
        "ExtraPerformanceTypeId": {
          "description": "额外性能的类型，取值如下：Balance：均衡型额外性能；IOPS：IOPS型额外性能；Throughput：吞吐量型额外性能。",
          "type": "string"
        },
        "IOPS": {
          "description": "云盘的额外IOPS。",
          "type": "number"
        },
        "Throughput": {
          "description": "云盘的额外吞吐量。",
          "type": "number"
        }
      },
      "type": "object"
    },
    "TotalPerformance": {
      "description": "总性能。",
      "properties": {
        "IOPS": {
          "description": "云盘的总IOPS，即云盘的基准IOPS和额外IOPS之和。",
          "type": "number"
        },
        "Throughput": {
          "description": "云盘的总吞吐量，即云盘的基准吞吐量和额外吞吐量之和。",
          "type": "number"
        }
      },
      "type": "object"
    }
  },
  "description": "云盘是高可用、高可靠、高性能、弹性扩展的块存储设备，可以作为云服务器和弹性容器服务的可扩展硬盘使用。",
  "handlers": {
    "create": {
      "permissions": [
        "storage_ebs:DescribeVolumes",
        "storage_ebs:CreateVolume",
        "storage_ebs:ApplyAutoSnapshotPolicy"
      ]
    },
    "delete": {
      "permissions": [
        "storage_ebs:DescribeVolumes",
        "storage_ebs:DeleteVolume"
      ]
    },
    "list": {
      "permissions": [
        "storage_ebs:DescribeVolumes"
      ]
    },
    "read": {
      "permissions": [
        "storage_ebs:DescribeVolumes"
      ]
    },
    "update": {
      "permissions": [
        "storage_ebs:DescribeVolumes",
        "storage_ebs:DeleteTags",
        "storage_ebs:CreateTags",
        "storage_ebs:ApplyAutoSnapshotPolicy",
        "storage_ebs:CancelAutoSnapshotPolicy",
        "storage_ebs:ModifyVolumeAttribute",
        "storage_ebs:ExtendVolume",
        "storage_ebs:ModifyVolumeExtraPerformance",
        "storage_ebs:ModifyVolumeSpec",
        "storage_ebs:DetachVolume",
        "storage_ebs:AttachVolume",
        "storage_ebs:ModifyVolumeChargeType"
      ]
    }
  },
  "primaryIdentifier": [
    "/properties/VolumeId"
  ],
  "properties": {
    "AutoSnapshotPolicyId": {
      "description": "自动快照策略ID。",
      "type": "string"
    },
    "AutoSnapshotPolicyName": {
      "description": "自动快照策略名称。",
      "type": "string"
    },
    "BaselinePerformance": {
      "$ref": "#/definitions/BaselinePerformance",
      "description": "基本性能。",
      "type": "object"
    },
    "CreatedAt": {
      "description": "云盘创建时间。",
      "semantic": "createdTime",
      "type": "string"
    },
    "DeleteWithInstance": {
      "default": false,
      "description": "释放实例时，该云盘是否随实例一起释放，取值说明如下：true：云盘随实例一起释放。false：云盘不随实例一起释放。",
      "type": "boolean"
    },
    "Description": {
      "description": "云盘的描述信息，默认为空，长度限制在1 ~ 255字符之间。",
      "maxLength": 255,
      "minLength": 0,
      "semantic": "description",
      "type": "string"
    },
    "DeviceName": {
      "description": "设备名称",
      "type": "string"
    },
    "ErrorDetail": {
      "description": "具体错误",
      "type": "string"
    },
    "ExpiredTime": {
      "description": "包年包月到期时间。",
      "type": "string"
    },
    "ExtraPerformance": {
      "$ref": "#/definitions/Performance",
      "description": "额外性能。",
      "type": "object"
    },
    "ImageId": {
      "description": "云盘中的镜像ID。",
      "type": "string"
    },
    "InstanceId": {
      "description": "创建云盘的同时挂载的ECS实例ID。",
      "type": "string"
    },
    "Kind": {
      "description": "云盘属性，data表示数据盘。默认值为data。说明： 仅支持创建数据盘。",
      "type": "string"
    },
    "OverdueReclaimTime": {
      "description": "云盘欠费回收时间。",
      "type": "string"
    },
    "OverdueTime": {
      "description": "云盘欠费关停时间。",
      "type": "string"
    },
    "PayType": {
      "description": "付费类型，说明如下： Pre：预付费（包年包月） Post：后付费（按量计费）。",
      "enum": [
        "pre",
        "post"
      ],
      "type": "string"
    },
    "ProjectName": {
      "description": "云盘所要加入的Project（项目）名称。",
      "semantic": "projectName",
      "type": "string"
    },
    "RenewType": {
      "description": "续费方式 1：手动续费 2 ：自动续费 3 ：到期不续费。",
      "enum": [
        0,
        1,
        2,
        3
      ],
      "type": "number"
    },
    "Size": {
      "description": "Size，数据盘容量大小，单位为GiB。不同类型的数据盘容量范围如下：ESSD_PL0：10~65536 GiB；ESSD_FlexPL：10~65536 GiB；TSSD_TL0：40~65536 GiB",
      "type": "number"
    },
    "SnapshotCount": {
      "description": "快照计数",
      "type": "number"
    },
    "SourceSnapshotId": {
      "description": "源快照。",
      "type": "string"
    },
    "Status": {
      "description": "云盘状态",
      "enum": [
        "available",
        "attaching",
        "attached",
        "detaching",
        "creating",
        "deleting",
        "error",
        "extending"
      ],
      "semantic": "status",
      "type": "string"
    },
    "TotalPerformance": {
      "$ref": "#/definitions/TotalPerformance",
      "description": "总性能。",
      "type": "object"
    },
    "TradeStatus": {
      "description": "交易状态 0：创建中 1：运行中 2：创建失败",
      "enum": [
        -1,
        0,
        1,
        2
      ],
      "type": "number"
    },
    "UpdatedAt": {
      "description": "云盘更新时间。",
      "semantic": "updatedTime",
      "type": "string"
    },
    "VolumeId": {
      "description": "云盘id。",
      "semantic": "resourceId",
      "type": "string"
    },
    "VolumeName": {
      "description": "云盘显示名称。命名规则如下：首字符仅支持中文、字母或下划线（_）。可包含中文、字母、数字、下划线（_）或中划线（-）。长度限制在1~128字符之间。",
      "maxLength": 128,
      "minLength": 1,
      "semantic": "resourceName",
      "type": "string"
    },
    "VolumeType": {
      "description": "云盘类型，取值说明如下：ESSD_PL0：极速型SSD云盘，PL0规格。ESSD_FlexPL: 极速型SSD云盘，FlexPL规格。TSSD_TL0: 吞吐型SSD云盘。",
      "enum": [
        "ESSD_PL0",
        "ESSD_FlexPL",
        "TSSD_TL0"
      ],
      "type": "string"
    },
    "ZoneId": {
      "description": "可用区ID。",
      "type": "string"
    }
  },
  "readOnlyProperties": [
    "/properties/VolumeId",
    "/properties/CreatedAt",
    "/properties/UpdatedAt",
    "/properties/ImageId",
    "/properties/Status",
    "/properties/OverdueReclaimTime",
    "/properties/OverdueTime",
    "/properties/TradeStatus",
    "/properties/ExpiredTime",
    "/properties/AutoSnapshotPolicyName",
    "/properties/DeviceName",
    "/properties/ErrorDetail",
    "/properties/SnapshotCount",
    "/properties/BaselinePerformance",
    "/properties/TotalPerformance",
    "/properties/RenewType"
  ],
  "required": [
    "Size",
    "VolumeName",
    "VolumeType",
    "ZoneId"
  ],
  "tagging": {
    "taggable": false
  },
  "typeName": "Volcengine::StorageEBS::Volume"
}