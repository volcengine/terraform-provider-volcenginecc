{
  "additionalProperties": false,
  "createOnlyProperties": [
    "/properties/Name",
    "/properties/Registry",
    "/properties/Project"
  ],
  "description": "命名空间是 OCI 制品仓库的集合，管理多个具有关联属性的 OCI 制品仓库。命名空间的名称通常为企业内部的组织团队名称、产品项目名称或个人自定义名称。",
  "filterProperties": {
    "filterableProperties": [
      "/properties/Project",
      "/properties/Registry",
      "/properties/Name"
    ],
    "required": [
      "/properties/Registry"
    ]
  },
  "handlers": {
    "create": {
      "permissions": [
        "cr:CreateNameSpace",
        "cr:ListNamespaces"
      ]
    },
    "delete": {
      "permissions": [
        "cr:DeleteNameSpace",
        "cr:ListNamespaces"
      ]
    },
    "list": {
      "permissions": [
        "cr:ListNamespaces"
      ]
    },
    "read": {
      "permissions": [
        "cr:ListNamespaces"
      ]
    }
  },
  "primaryIdentifier": [
    "/properties/Registry",
    "/properties/Name"
  ],
  "properties": {
    "CreatedTime": {
      "description": "命名空间的创建时间。",
      "semantic": "createdTime",
      "type": "string"
    },
    "Name": {
      "description": "命名空间名称。支持小写英文、数字、英文句号（.）、短划线（-）、下划线（_），标点符号不能出现在首位或末位，也不能连续输入。长度限制为 2～90 个字符。标准版实例：同一个镜像仓库实例下，名称必须唯一。体验版实例：同一地域下，所有账号范围内的命名空间名称需要保持唯一。如果您设置的命名空间名称已被占用，请尝试其他名称或者 购买标准版实例。",
      "maxLength": 90,
      "minLength": 2,
      "semantic": "resourceId",
      "type": "string"
    },
    "Project": {
      "default": "default",
      "description": "命名空间所属项目的名称。参数值大小写敏感，不得超过 64 个字符。参数为空时，命名空间关联默认的 default项目。",
      "maxLength": 64,
      "minLength": 1,
      "semantic": "projectName",
      "type": "string"
    },
    "Registry": {
      "description": "指定的镜像仓库实例名称。通过 ListRegistries 或在 镜像仓库控制台 的 实例列表页面获取。",
      "type": "string"
    }
  },
  "readOnlyProperties": [
    "/properties/CreatedTime"
  ],
  "required": [
    "Registry",
    "Name"
  ],
  "tagging": {
    "taggable": false
  },
  "typeName": "Volcengine::CR::NameSpace"
}