{
  "additionalProperties": false,
  "createOnlyProperties": [
    "/properties/ProjectName",
    "/properties/VpcId"
  ],
  "definitions": {
    "AclRuleEntry": {
      "properties": {
        "CidrIp": {
          "description": "入向规则时为源地址的网段。出向规则时为目标地址的网段。支持CIDR格式和IPv4格式的IP地址范围。默认值：无。",
          "type": "string"
        },
        "Description": {
          "description": "规则的描述信息。",
          "type": "string"
        },
        "NetworkAclEntryId": {
          "description": "规则的ID。",
          "type": "string"
        },
        "NetworkAclEntryName": {
          "description": "规则的名称。",
          "type": "string"
        },
        "Policy": {
          "description": "授权策略。accept：允许访问。drop：拒绝访问，不返回拒绝访问的信息，仅表现出发起端请求超时或类似无法建立连接的信息。",
          "enum": [
            "accept",
            "drop"
          ],
          "type": "string"
        },
        "Port": {
          "description": "规则的目的端口范围。当方向规则的Protocol为all、icmp或gre时，端口范围为-1/-1，表示不限制端口。当方向规则的Protocol为tcp或udp时，端口范围为1~65535，格式为1/200、80/80，表示端口1到端口200、端口80。",
          "type": "string"
        },
        "Priority": {
          "description": "方向规则的优先级，数字越小，代表优先级越高。不填默认值：1。",
          "format": "int64",
          "type": "integer"
        },
        "Protocol": {
          "description": "协议类型。tcp：TCP协议。udp：UDP协议。icmp：ICMP协议。icmpv6：ICMPV6协议。gre：GRE协议。all：支持所有协议。",
          "enum": [
            "tcp",
            "udp",
            "icmp",
            "icmpv6",
            "gre",
            "all"
          ],
          "type": "string"
        }
      },
      "required": [
        "Policy",
        "Port",
        "CidrIp",
        "NetworkAclEntryName",
        "Protocol"
      ],
      "type": "object"
    },
    "Resource": {
      "properties": {
        "ResourceId": {
          "description": "关联资源的ID。",
          "type": "string"
        },
        "Status": {
          "description": "网络ACL关联资源的状态。BINDED：已绑定。BINDING：绑定中。UNBINDING：解绑中。",
          "type": "string"
        }
      },
      "type": "object"
    },
    "Tag": {
      "properties": {
        "Key": {
          "description": "用户标签的标签键。",
          "type": "string"
        },
        "Value": {
          "description": "用户标签的标签值。",
          "type": "string"
        }
      },
      "required": [
        "Key"
      ],
      "type": "object"
    }
  },
  "description": "网络ACL用于控制子网的出入流量，其基于白名单原理设计，出入子网的流量必须被网络ACL规则允许才能放通。",
  "filterProperties": {
    "filterableProperties": [
      "/properties/ProjectName",
      "/properties/NetworkAclId",
      "/properties/NetworkAclName",
      "/properties/VpcId",
      "/properties/Resources/*/ResourceId",
      "/properties/Tags/*/Key",
      "/properties/Tags/*/Value"
    ]
  },
  "handlers": {
    "create": {
      "permissions": [
        "vpc:DescribeNetworkAclAttributes",
        "vpc:CreateNetworkAcl",
        "vpc:AssociateNetworkAcl",
        "vpc:UpdateNetworkAclEntries"
      ]
    },
    "delete": {
      "permissions": [
        "vpc:DescribeNetworkAclAttributes",
        "vpc:DisassociateNetworkAcl",
        "vpc:DeleteNetworkAcl"
      ]
    },
    "list": {
      "permissions": [
        "vpc:DescribeNetworkAcls"
      ]
    },
    "read": {
      "permissions": [
        "vpc:DescribeNetworkAclAttributes"
      ]
    },
    "update": {
      "permissions": [
        "vpc:DescribeNetworkAclAttributes",
        "vpc:ModifyNetworkAclAttributes",
        "vpc:UpdateNetworkAclEntries",
        "vpc:AssociateNetworkAcl",
        "vpc:DisassociateNetworkAcl",
        "vpc:UntagResources",
        "vpc:TagResources"
      ]
    }
  },
  "primaryIdentifier": [
    "/properties/NetworkAclId"
  ],
  "properties": {
    "CreatedTime": {
      "description": "网络ACL的创建时间。",
      "semantic": "createdTime",
      "type": "string"
    },
    "DefaultEgressAclEntries": {
      "description": "默认出向ACL规则列表。",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/AclRuleEntry"
      },
      "type": "array",
      "uniqueItems": true
    },
    "DefaultIngressAclEntries": {
      "description": "默认入向ACL规则列表。",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/AclRuleEntry"
      },
      "type": "array",
      "uniqueItems": true
    },
    "Description": {
      "description": "网络ACL描述信息。长度限制为0~ 255个字符。需要以字母、中文或数字开头。可包含英文逗号（,）、点号（.）、下划线（_）、空格（ ）、等号（=）、短横线（-）、中文逗号（，）、中文句号（。）。",
      "maxLength": 255,
      "semantic": "description",
      "type": "string"
    },
    "EgressAclEntries": {
      "description": "出向ACL规则列表。",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/AclRuleEntry"
      },
      "type": "array",
      "uniqueItems": true
    },
    "IngressAclEntries": {
      "description": "入向ACL规则列表。",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/AclRuleEntry"
      },
      "type": "array",
      "uniqueItems": true
    },
    "NetworkAclId": {
      "description": "网络ACL的ID。",
      "semantic": "resourceId",
      "type": "string"
    },
    "NetworkAclName": {
      "description": "网络ACL名称。长度限制为1 ~ 128个字符。需要以字母、中文或数字开头，可包含点号（.）、下划线（_）和短横线（-）。",
      "maxLength": 128,
      "minLength": 1,
      "semantic": "resourceName",
      "type": "string"
    },
    "ProjectName": {
      "default": "default",
      "description": "网络ACL所属项目的名称。不填默认加入default项目。",
      "semantic": "projectName",
      "type": "string"
    },
    "Resources": {
      "description": "关联资源列表。",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/Resource"
      },
      "type": "array",
      "uniqueItems": true
    },
    "Status": {
      "description": "网络ACL的状态。Available：可用，Creating：创建中",
      "semantic": "status",
      "type": "string"
    },
    "Tags": {
      "description": "标签列表。",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/Tag"
      },
      "semantic": "tags",
      "type": "array",
      "uniqueItems": true
    },
    "UpdatedTime": {
      "description": "网络ACL的更新时间。",
      "semantic": "updatedTime",
      "type": "string"
    },
    "VpcId": {
      "description": "网络ACL所属VPC的ID。",
      "type": "string"
    }
  },
  "readOnlyProperties": [
    "/properties/NetworkAclId",
    "/properties/Status",
    "/properties/CreatedTime",
    "/properties/UpdatedTime",
    "/properties/Resources/*/Status",
    "/properties/DefaultEgressAclEntries",
    "/properties/DefaultIngressAclEntries",
    "/properties/IngressAclEntries/*/Priority",
    "/properties/IngressAclEntries/*/NetworkAclEntryId",
    "/properties/EgressAclEntries/*/Priority",
    "/properties/EgressAclEntries/*/NetworkAclEntryId"
  ],
  "required": [
    "VpcId"
  ],
  "tagging": {
    "tagOnCreate": true,
    "tagUpdatable": true,
    "taggable": true
  },
  "typeName": "Volcengine::VPC::NetworkAcl"
}