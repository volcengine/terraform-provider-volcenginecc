{
  "additionalProperties": false,
  "createOnlyProperties": [
    "/properties/Description",
    "/properties/Name",
    "/properties/ZoneId"
  ],
  "description": "高性能计算集群（High Performance Compute Cluster，HPC集群）是一种用于处理大规模计算任务的集群计算环境，通常由大量的计算节点组成，这些节点通过RDMA（Remote Direct Memory Access）高速网络互联，并协同工作以处理复杂的计算任务。高性能计算集群可以提供强大的计算能力、大内存、高速存储和高速网络，以满足对计算密集型、数据密集型或两者结合的工作负载的需求。",
  "handlers": {
    "create": {
      "permissions": [
        "ecs:CreateHpcCluster",
        "ecs:DescribeHpcClusters"
      ]
    },
    "delete": {
      "permissions": [
        "ecs:DeleteHpcCluster",
        "ecs:DescribeHpcClusters"
      ]
    },
    "list": {
      "permissions": [
        "ecs:DescribeHpcClusters"
      ]
    },
    "read": {
      "permissions": [
        "ecs:DescribeHpcClusters"
      ]
    }
  },
  "primaryIdentifier": [
    "/properties/HpcClusterId"
  ],
  "properties": {
    "CreatedTime": {
      "description": "创建时间，格式满足RFC3339。",
      "type": "string"
    },
    "Description": {
      "description": "高性能计算集群描述，默认为空字符串。必须以字母或中文开头。只能包含中文、字母、数字、下划线和中划线。长度限制在0～255之间。",
      "type": "string"
    },
    "HpcClusterId": {
      "description": "高性能计算集群ID。",
      "type": "string"
    },
    "Name": {
      "description": "高性能计算集群名称。必须以字母或中文开头。只能包含中文、字母、数字、下划线和中划线。长度限制在1～128之间。",
      "type": "string"
    },
    "UpdatedTime": {
      "description": "更新时间，格式满足RFC3339。",
      "type": "string"
    },
    "VpcId": {
      "description": "私有网络ID。",
      "type": "string"
    },
    "ZoneId": {
      "description": "高性能计算集群所属可用区ID。",
      "type": "string"
    }
  },
  "readOnlyProperties": [
    "/properties/HpcClusterId",
    "/properties/VpcId",
    "/properties/CreatedAt",
    "/properties/UpdatedAt"
  ],
  "required": [
    "Name",
    "ZoneId"
  ],
  "tagging": {
    "taggable": false
  },
  "typeName": "Volcengine::ECS::HpcCluster"
}