          "type": "string"
        },
        "Ipv4LocalAddresses": {
          "description": "NLB实例的私网IPv4 LIP地址。",
          "insertionOrder": false,
          "items": {
            "type": "string"
          },
          "type": "array",
          "uniqueItems": true
        },
        "Ipv6Address": {
          "description": "NLB实例的私网IPv6 VIP地址。",
          "type": "string"
        },
        "Ipv6EipId": {
          "description": "NLB实例关联的IPv6公网带宽的ID。",
          "type": "string"
        },
        "Ipv6HcStatus": {
          "description": "NLB实例私网IPv6地址的健康状态。Healthy：健康。Unhealthy：异常。",
