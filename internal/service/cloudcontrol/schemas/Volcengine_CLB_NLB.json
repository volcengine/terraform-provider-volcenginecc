        },
        "Ipv6LocalAddresses": {
          "description": "NLB实例的私网IPv6 LIP地址。",
          "insertionOrder": false,
          "items": {
            "type": "string"
          },
          "type": "array",
          "uniqueItems": true
        },
        "SubnetId": {
          "description": "NLB实例所属的子网ID。",
