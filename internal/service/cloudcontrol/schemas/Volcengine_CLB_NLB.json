        },
        "ZoneId": {
          "description": "NLB实例所属的可用区ID。",
          "type": "string"
        }
      },
      "type": "object"
    }
  },
  "description": "NLB 实例用于接收用户的访问请求，并根据监听器和服务器组的配置设定，将访问请求转发至后端服务器。",
  "handlers": {
    "create": {
      "permissions": [
        "clb:CreateNetworkLoadBalancer",
        "clb:DescribeNetworkLoadBalancerAttributes"
      ]
    },
    "delete": {
      "permissions": [
        "clb:DeleteNetworkLoadBalancer",
        "clb:DescribeNetworkLoadBalancerAttributes"
      ]
    },
    "list": {
      "permissions": [
        "clb:DescribeNetworkLoadBalancers"
      ]
    },
    "read": {
      "permissions": [
        "clb:DescribeNetworkLoadBalancerAttributes"
      ]
    },
    "update": {
      "permissions": [
        "clb:ModifyNetworkLoadBalancerAttributes",
        "clb:ModifyNetworkLoadBalancerZones",
        "clb:ModifyNetworkLoadBalancerSecurityGroups",
        "clb:ModifyNetworkLoadBalancerNetworkType",
        "clb:ModifyNetworkLoadBalancerBandwidthPackage",
        "clb:UntagNLBResources",
        "clb:TagNLBResources",
        "clb:DescribeNetworkLoadBalancerAttributes"
      ]
    }
  },
  "primaryIdentifier": [
    "/properties/LoadBalancerId"
  ],
  "properties": {
    "AccountId": {
      "description": "NLB实例所属的账号ID。",
      "type": "string"
    },
    "BillingStatus": {
      "description": "NLB实例的计费状态。Normal: 正常，FinancialLocked: 被锁定。",
      "enum": [
        "Normal",
        "FinancialLocked"
      ],
      "type": "string"
    },
    "BillingType": {
      "description": "NLB实例的计费类型。3：按使用量计费",
      "enum": [
        3
      ],
      "format": "int64",
      "type": "integer"
    },
    "CreatedTime": {
      "description": "NLB实例的创建时间。",
      "type": "string"
    },
    "CrossZoneEnabled": {
      "default": true,
      "description": "NLB实例是否开启跨可用区转发能力。true（默认值）：开启。false：关闭。",
      "type": "boolean"
    },
    "DNSName": {
      "description": "NLB实例的DNS地址。",
      "type": "string"
    },
    "Description": {
      "description": "NLB实例的描述。必须以字母、数字或中文开头，可包含以下特殊字符：英文逗号（,）、点（.）、下划线（_）、空格（ ）、等号（=）、中划线（-）、中文逗号（，）、中文句号（。）。长度限制为0 ～ 255个字符。该参数不传入，则默认为空字符串。",
      "maxLength": 255,
      "type": "string"
    },
    "ExpectedOverdueTime": {
      "description": "NLB实例欠费关停时间。",
      "type": "string"
    },
    "IpAddressVersion": {
      "description": "NLB实例的IP地址类型。ipv4：IPv4版本。dualstack：双栈，即同时支持IPv4版本和IPv6版本。",
      "enum": [
        "ipv4",
        "dualstack"
      ],
      "type": "string"
    },
    "Ipv4BandwidthPackageId": {
      "description": "IPv4类型的共享带宽包ID。",
      "type": "string"
    },
    "Ipv4NetworkType": {
      "description": "NLB实例的IPv4网络类型。internet：公网类型。intranet：私网类型。",
      "enum": [
        "internet",
        "intranet"
      ],
      "type": "string"
    },
    "Ipv6BandwidthPackageId": {
      "description": "IPv6类型的共享带宽包ID。",
      "type": "string"
    },
    "Ipv6NetworkType": {
      "description": "NLB实例的IPv6网络类型。internet：公网类型。intranet：私网类型。",
      "enum": [
        "internet",
        "intranet"
      ],
      "type": "string"
    },
    "LoadBalancerId": {
      "description": "NLB实例ID。",
      "type": "string"
    },
    "LoadBalancerName": {
      "description": "NLB实例的名称。",
      "type": "string"
    },
    "ManagedSecurityGroupId": {
      "description": "系统为NLB实例自动关联的托管安全组ID。",
      "type": "string"
    },
    "ModificationProtectionStatus": {
      "description": "NLB实例是否开启控制台上修改保护。NonProtection或空：关闭，允许通过控制台修改实例或删除实例。ConsoleProtection：开启，禁止通过控制台修改实例或删除实例。",
      "enum": [
        "NonProtection",
        "ConsoleProtection",
        ""
      ],
      "type": "string"
    },
    "OverdueTime": {
      "description": "NLB实例被锁定的时间。参数BillingStatus为Normal时，本参数返回为空字符串。",
      "type": "string"
    },
    "ProjectName": {
      "description": "NLB实例所属项目的名称。",
      "type": "string"
    },
    "ReclaimedTime": {
      "description": "NLB实例的预期回收时间。参数BillingStatus为Normal时，本参数返回为空字符串。",
      "type": "string"
    },
    "SecurityGroupIds": {
      "description": "NLB实例关联的安全组ID列表。",
      "insertionOrder": false,
      "items": {
        "type": "string"
      },
      "type": "array",
      "uniqueItems": true
    },
    "Status": {
      "description": "NLB 实例的状态。Inactive：已停止。Active：运行中。Creating：创建中。Provisioning：创建中。仅调用API创建NLB实例时存在该状态。Configuring：配置中。Deleting：删除中。CreateFailed：创建失败。",
      "enum": [
        "Inactive",
        "Active",
        "Creating",
        "Provisioning",
        "Configuring",
        "Deleting",
        "CreateFailed"
      ],
      "type": "string"
    },
    "Tags": {
      "description": "NLB实例的标签信息。",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/Tag"
      },
      "type": "array",
      "uniqueItems": true
    },
    "UpdatedTime": {
      "description": "NLB实例的更新时间。",
      "type": "string"
    },
    "VpcId": {
      "description": "NLB实例所属的VPC ID。",
      "type": "string"
    },
    "ZoneMappings": {
      "description": "NLB实例的可用区信息。",
      "items": {
        "$ref": "#/definitions/ZoneMapping"
      },
      "type": "array"
    }
  },
  "readOnlyProperties": [
    "/properties/UpdatedTime",
    "/properties/CreatedTime",
    "/properties/BillingStatus",
    "/properties/BillingType",
    "/properties/DNSName",
    "/properties/ExpectedOverdueTime",
    "/properties/LoadBalancerId",
    "/properties/ManagedSecurityGroupId",
    "/properties/OverdueTime",
    "/properties/ReclaimedTime",
    "/properties/Status"
  ],
  "required": [
    "Ipv4NetworkType",
    "SecurityGroupIds",
    "VpcId",
    "ZoneMappings"
  ],
  "tagging": {
    "tagOnCreate": true,
    "tagUpdatable": true,
    "taggable": true
  },
  "typeName": "Volcengine::CLB::NLB"
}