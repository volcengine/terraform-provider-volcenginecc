{
  "additionalProperties": false,
  "createOnlyProperties": [
    "/properties/TransitRouterId",
    "/properties/VpcId"
  ],
  "definitions": {
    "AttachPoint": {
      "properties": {
        "NetworkInterfaceId": {
          "description": "连接点的网卡ID。",
          "type": "string"
        },
        "SubnetId": {
          "description": "连接点的子网ID。",
          "type": "string"
        },
        "ZoneId": {
          "description": "连接点的可用区ID。",
          "type": "string"
        }
      },
      "required": [
        "SubnetId",
        "ZoneId"
      ],
      "type": "object"
    },
    "Tag": {
      "properties": {
        "Key": {
          "description": "用户标签的标签键。",
          "type": "string"
        },
        "Value": {
          "description": "用户标签的标签值。",
          "type": "string"
        }
      },
      "required": [
        "Key"
      ],
      "type": "object"
    }
  },
  "description": "通过在私有网络和中转路由器之间建立连接，可以实现私有网络与中转路由器的私网互通。",
  "filterProperties": {
    "filterableProperties": [
      "/properties/TransitRouterId",
      "/properties/TransitRouterAttachmentId",
      "/properties/TransitRouterId",
      "/properties/VpcId",
      "/properties/Tags/*/Key",
      "/properties/Tags/*/Value"
    ],
    "required": [
      "/properties/TransitRouterId"
    ]
  },
  "handlers": {
    "create": {
      "permissions": [
        "transitrouter:CreateTransitRouterVpcAttachment",
        "transitrouter:DescribeTransitRouterVpcAttachments"
      ]
    },
    "delete": {
      "permissions": [
        "transitrouter:DeleteTransitRouterVpcAttachment",
        "transitrouter:DescribeTransitRouterVpcAttachments"
      ]
    },
    "list": {
      "permissions": [
        "transitrouter:DescribeTransitRouterVpcAttachments"
      ]
    },
    "read": {
      "permissions": [
        "transitrouter:DescribeTransitRouterVpcAttachments"
      ]
    },
    "update": {
      "permissions": [
        "transitrouter:ModifyTransitRouterVpcAttachmentAttributes",
        "transitrouter:DescribeTransitRouterVpcAttachments",
        "transitrouter:UntagResources",
        "transitrouter:TagResources"
      ]
    }
  },
  "primaryIdentifier": [
    "/properties/TransitRouterId",
    "/properties/TransitRouterAttachmentId"
  ],
  "properties": {
    "ApplianceModeEnabled": {
      "description": "是否启动路径一致模式。请严格按照以下枚举值的大小写输入，不要传入其他取值。true：是。开启后，TR转发流量至该VPC连接时，将选择相同的可用区连接点转发请求流量和返回流量。false：否。",
      "type": "boolean"
    },
    "AttachPoints": {
      "description": "网络实例连接列表。",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/AttachPoint"
      },
      "type": "array",
      "uniqueItems": true
    },
    "AutoPublishRouteEnabled": {
      "description": "是否自动同步TR路由到网络实例路由表中。请严格按照以下枚举值的大小写输入，不要传入其他取值。true：是，表示系统会自动同步该网络实例连接关联转发的TR路由表中的路由条目到网络实例的路由表中，仅当该参数配置为true，且网络实例连接关联转发的TR路由表中存在路由条目时，系统才会自动同步TR路由到网络实例的路由表中。false（默认值）：否。",
      "type": "boolean"
    },
    "Bandwidth": {
      "description": "网络实例连接的带宽上限，单位为Gbps。",
      "type": "integer"
    },
    "CreatedTime": {
      "description": "创建时间。",
      "semantic": "createdTime",
      "type": "string"
    },
    "Description": {
      "description": "网络实例连接的描述信息。必须以字母、数字或中文开头，可包含字母、数字、中文和以下特殊字符：英文逗号（,）、点号（.）、下划线（_）、空格（ ）、等号（=）、短横线（-）、中文逗号（，）、中文句号（。）。长度限制为0 ~ 255个字符。不传入该参数或该参数不传入数值时，默认为空字符串。",
      "maxLength": 255,
      "semantic": "description",
      "type": "string"
    },
    "Ipv6Enabled": {
      "description": "是否开启IPv6功能。请严格按照以下枚举值的大小写输入，不要传入其他取值。true：是。false：否。",
      "type": "boolean"
    },
    "Status": {
      "description": "网络实例连接的状态。Creating: 创建中。Deleting: 删除中。Pending：配置中。Available：可用。",
      "semantic": "status",
      "type": "string"
    },
    "Tags": {
      "description": "标签列表。",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/Tag"
      },
      "semantic": "tags",
      "type": "array",
      "uniqueItems": true
    },
    "TransitRouterAttachmentId": {
      "description": "网络实例连接的ID。",
      "semantic": "resourceId",
      "type": "string"
    },
    "TransitRouterAttachmentName": {
      "description": "网络实例连接的名称。必须以字母、数字或中文开头，可包含字母、数字、中文和以下特殊字符：点号（.）、下划线（_）和短横线（-）。长度限制为1 ~ 128个字符。不传入该参数或该参数不传入数值时，默认为网络实例连接的ID。",
      "maxLength": 128,
      "semantic": "resourceName",
      "type": "string"
    },
    "TransitRouterId": {
      "description": "中转路由器实例的ID。",
      "type": "string"
    },
    "UpdatedTime": {
      "description": "更新时间。",
      "semantic": "updatedTime",
      "type": "string"
    },
    "VpcId": {
      "description": "私有网络实例的ID。",
      "type": "string"
    }
  },
  "readOnlyProperties": [
    "/properties/TransitRouterAttachmentId",
    "/properties/CreatedTime",
    "/properties/UpdatedTime",
    "/properties/Status",
    "/properties/Bandwidth",
    "/properties/AttachPoints/*/NetworkInterfaceId"
  ],
  "required": [
    "TransitRouterId",
    "VpcId",
    "AttachPoints"
  ],
  "tagging": {
    "tagOnCreate": true,
    "tagUpdatable": true,
    "taggable": true
  },
  "typeName": "Volcengine::TransitRouter::VpcAttachment"
}