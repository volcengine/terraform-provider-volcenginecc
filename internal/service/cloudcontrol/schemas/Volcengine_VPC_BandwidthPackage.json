    "/properties/BandwidthPackageId",
    "/properties/CreatedTime",
    "/properties/DeletedTime",
    "/properties/ExpiredTime",
    "/properties/OverdueTime",
    "/properties/Ratio",
    "/properties/Status",
    "/properties/UpdatedTime"
  ],
  "required": [
    "Bandwidth"
  ],
  "tagging": {
    "tagOnCreate": true,
    "tagUpdatable": true,
    "taggable": true
  },
  "typeName": "Volcengine::VPC::BandwidthPackage",
  "writeOnlyProperties": [
