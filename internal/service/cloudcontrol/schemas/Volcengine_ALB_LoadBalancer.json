          "maximum": 500,
          "minimum": 1,
          "type": "integer"
        },
        "BillingType": {
          "default": 2,
          "description": "EIP的计费方式，2为按带宽计费，3为按流量计费。",
          "enum": [
            2,
            3
          ],
          "format": "int64",
          "type": "integer"
        },
        "ISP": {
          "default": "BGP",
          "description": "公网IP的线路类型，BGP表示多线。",
          "enum": [
            "BGP"
          ],
          "type": "string"
        }
      },
