    "Ipv6EipBillingConfig": {
      "description": "IPv6弹性公网IP的详细计费配置。",
      "properties": {
        "Bandwidth": {
          "default": 1,
          "description": "IPv6 EIP的带宽峰值，单位为Mbps。",
          "format": "int64",
          "maximum": 500,
          "minimum": 1,
          "type": "integer"
        },
        "BillingType": {
          "default": 3,
          "description": "IPv6 EIP的计费方式，2为按带宽计费，3为按流量计费。",
