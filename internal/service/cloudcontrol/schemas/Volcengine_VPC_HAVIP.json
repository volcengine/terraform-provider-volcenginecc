      "type": "string"
    },
    "CreatedAt": {
      "description": "HAVIP的创建时间。",
      "semantic": "createdTime",
      "type": "string"
    },
    "Description": {
      "description": "HAVIP的描述。长度限制为0～255个字符。",
      "maxLength": 255,
      "minLength": 0,
      "semantic": "description",
      "type": "string"
    },
    "HaVipId": {
      "description": "HAVIP的ID。",
      "semantic": "resourceId",
      "type": "string"
    },
    "HaVipName": {
      "description": "HAVIP的名称。必须以字母、数字或中文开头，可包含字母、数字、中文及以下特殊字符：点号（.）、下划线（_）和中划线（-）。长度限制在0～128字符之间。",
      "maxLength": 128,
      "minLength": 0,
      "semantic": "resourceName",
      "type": "string"
    },
    "IpAddress": {
      "description": "HAVIP的IP地址。",
      "type": "string"
    },
