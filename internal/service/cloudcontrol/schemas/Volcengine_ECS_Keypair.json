{
  "additionalProperties": false,
  "createOnlyProperties": [
    "/properties/KeyPairName ",
    "/properties/ProjectName "
  ],
  "definitions": {
    "Tag": {
      "properties": {
        "Key": {
          "description": "标签键。",
          "type": "string"
        },
        "Value": {
          "description": "标签值。",
          "type": "string"
        }
      },
      "required": [
        "Key",
        "Value"
      ],
      "type": "object"
    }
  },
  "description": "ECS 密钥对（Keypair）默认采用 RSA 2048 位加密算法生成一对关联密钥，包含公钥（public key）和私钥（private key）。其中，公钥用于对数据进行加密处理，可将明文转换为不可直接读取的密文；私钥则作为解密的唯一凭证，能将密文还原为原始明文，实现安全的身份验证与数据传输。",
  "handlers": {
    "create": {
      "permissions": [
        "ecs:CreateKeyPair",
        "ecs:AttachKeyPair",
        "ecs:ImportKeyPair",
        "ecs:DescribeKeyPairs",
        "ecs:DescribeInstances"
      ]
    },
    "delete": {
      "permissions": [
        "ecs:DetachKeyPair",
        "ecs:DeleteKeyPair",
        "ecs:DescribeKeyPairs",
        "ecs:DescribeInstances"
      ]
    },
    "list": {
      "permissions": [
        "ecs:DescribeKeyPairs "
      ]
    },
    "read": {
      "permissions": [
        "ecs:DescribeKeyPairs",
        "ecs:DescribeInstances"
      ]
    },
    "update": {
      "permissions": [
        "ecs:ModifyKeyPairAttribute",
        "ecs:AttachKeyPair",
        "ecs:DetachKeyPair",
        "ecs:DescribeKeyPairs",
        "ecs:DescribeInstances"
      ]
    }
  },
  "primaryIdentifier": [
    "/properties/KeyPairName"
  ],
  "properties": {
    "CreatedTime": {
      "description": "密钥对创建时间。",
      "semantic": "createdTime",
      "type": "string"
    },
    "Description": {
      "description": "密钥对的描述，默认值为空字符串。必须以字母或中文开头。只能包含中文、字母、数字、点“.”、空格、下划线“_”、中划线“-”、等号“=”、英文逗号“,”、中文逗号“，”和中文句号“。”长度限制在255个字符以内。",
      "semantic": "description",
      "type": "string"
    },
    "FingerPrint": {
      "description": "密钥对的指纹。根据RFC4716定义的公钥指纹格式，采用MD5信息摘要算法。",
      "type": "string"
    },
    "InstanceIds": {
      "description": "操作的实例ID。",
      "insertionOrder": true,
      "items": {
        "type": "string"
      },
      "type": "array",
      "uniqueItems": true
    },
    "KeyPairId": {
      "description": "密钥对唯一ID。",
      "semantic": "resourceId",
      "type": "string"
    },
    "KeyPairName": {
      "description": "密钥对名称。不可与已有名称重复。长度限制在 2～64 个字符之间。允许使用点号“.”分隔字符成多段，每段允许使用大小写字母、数字或连字符“-”。不能以“-”和“.”开头或结尾，不能连续使用“-”或者“.”。",
      "semantic": "resourceName",
      "type": "string"
    },
    "PrivateKey": {
      "description": "密钥对私钥信息。",
      "type": "string"
    },
    "ProjectName": {
      "description": "资源所属项目，一个资源只能归属于一个项目。只能包含字母、数字、下划线“_”、点“.”和中划线“-”。长度限制在64个字符以内。",
      "semantic": "projectName",
      "type": "string"
    },
    "PublicKey": {
      "description": "密钥对的公钥信息。",
      "type": "string"
    },
    "Tags": {
      "description": "密钥对的标签。",
      "insertionOrder": true,
      "items": {
        "$ref": "#/definitions/Tag"
      },
      "semantic": "tags",
      "type": "array",
      "uniqueItems": true
    },
    "UpdatedTime": {
      "description": "密钥对更新时间。",
      "semantic": "updatedTime",
      "type": "string"
    }
  },
  "readOnlyProperties": [
    "/properties/CreatedTime",
    "/properties/FingerPrint",
    "/properties/KeyPairId",
    "/properties/UpdatedTime"
  ],
  "required": [
    "KeyPairName"
  ],
  "tagging": {
    "tagOnCreate": true,
    "tagUpdatable": true,
    "taggable": true
  },
  "typeName": "Volcengine::ECS::Keypair",
  "writeOnlyProperties": [
    "/properties/PrivateKey",
    "/properties/PublicKey"
  ]
}