{
  "additionalProperties": false,
  "createOnlyProperties": [
    "/properties/RegionId",
    "/properties/MasterZoneId",
    "/properties/SlaveZoneId",
    "/properties/ProjectName",
    "/properties/VpcId",
    "/properties/SubnetId",
    "/properties/ZoneType",
    "/properties/Eip/ISP",
    "/properties/Eip/EipBillingType",
    "/properties/Eip/Bandwidth",
    "/properties/Eip/BandwidthPackageId",
    "/properties/Eip/SecurityProtectionTypes",
    "/properties/Eip/SecurityProtectionInstanceId",
    "/properties/EniAddressNum"
  ],
  "definitions": {
    "AccessLog": {
      "description": "负载均衡实例中访问日志的信息",
      "properties": {
        "BucketName": {
          "description": "存储七层访问日志的对象存储桶的名称",
          "type": "string"
        },
        "DeliveryType": {
          "description": "日志投递类型",
          "enum": [
            "tos",
            "tls"
          ],
          "type": "string"
        },
        "Enabled": {
          "description": "是否开启访问日志（七层）投递至对象存储TOS的功能",
          "type": "boolean"
        },
        "TlsProjectId": {
          "description": "日志项目",
          "type": "string"
        },
        "TlsTopicId": {
          "description": "日志主题",
          "type": "string"
        }
      },
      "type": "object"
    },
    "Eip": {
      "description": "公网IP配置",
      "properties": {
        "Bandwidth": {
          "description": "带宽峰值",
          "type": "number"
        },
        "BandwidthPackageId": {
          "description": "共享带宽包ID",
          "type": "string"
        },
        "EipBillingType": {
          "description": "计费方式",
          "maximum": 3,
          "minimum": 1,
          "type": "number"
        },
        "ISP": {
          "description": "线路类型",
          "type": "string"
        },
        "SecurityProtectionInstanceId": {
          "description": "防护包ID",
          "type": "number"
        },
        "SecurityProtectionTypes": {
          "description": "安全防护类型",
          "insertionOrder": false,
          "items": {
            "type": "string"
          },
          "type": "array",
          "uniqueItems": true
        }
      },
      "type": "object"
    },
    "Eni": {
      "description": "负载均衡实例的私网IPv4地址信息",
      "properties": {
        "EniAddress": {
          "description": "负载均衡实例的私网IP地址",
          "semantic": "privateIp",
          "type": "string"
        },
        "EniID": {
          "description": "负载均衡实例占用的网卡ID",
          "type": "string"
        }
      },
      "type": "object"
    },
    "Ipv6AddressBandwidth": {
      "description": "负载均衡实例的IPv6公网带宽信息",
      "properties": {
        "Bandwidth": {
          "description": "IPv6公网带宽的带宽峰值，单位为Mbps",
          "type": "number"
        },
        "BandwidthPackageId": {
          "description": "IPv6公网带宽关联的共享带宽包ID",
          "type": "string"
        },
        "BillingType": {
          "description": "IPv6公网带宽的计费类型",
          "type": "number"
        },
        "ISP": {
          "description": "IPv6公网带宽的线路类型",
          "type": "string"
        },
        "NetworkType": {
          "description": "负载均衡实例IPv6地址的网络类型",
          "type": "string"
        }
      },
      "type": "object"
    },
    "Listener": {
      "description": "监听器信息",
      "properties": {
        "ListenerId": {
          "description": "监听器的ID",
          "type": "string"
        },
        "ListenerName": {
          "description": "监听器的名称",
          "type": "string"
        }
      },
      "required": [
        "ListenerId",
        "ListenerName"
      ],
      "type": "object"
    },
    "ServerGroup": {
      "description": "监听器信息",
      "properties": {
        "ServerGroupId": {
          "description": "负载均衡实例中后端服务器组的ID",
          "type": "string"
        },
        "ServerGroupName": {
          "description": "负载均衡实例中后端服务器组的名称",
          "type": "string"
        }
      },
      "required": [
        "ServerGroupId",
        "ServerGroupName"
      ],
      "type": "object"
    },
    "Tag": {
      "description": "资源标签",
      "properties": {
        "Key": {
          "description": "标签键",
          "type": "string"
        },
        "Value": {
          "description": "标签值",
          "type": "string"
        }
      },
      "required": [
        "Key",
        "Value"
      ],
      "type": "object"
    }
  },
  "description": "负载均衡实例",
  "handlers": {
    "create": {
      "permissions": [
        "clb:CreateLoadBalancer",
        "clb:DescribeLoadBalancerAttributes"
      ]
    },
    "delete": {
      "permissions": [
        "clb:DeleteLoadBalancer",
        "clb:DescribeLoadBalancerAttributes"
      ]
    },
    "list": {
      "permissions": [
        "clb:DescribeLoadBalancers"
      ]
    },
    "read": {
      "permissions": [
        "clb:DescribeLoadBalancerAttributes"
      ]
    },
    "update": {
      "permissions": [
        "clb:DescribeLoadBalancerAttributes",
        "clb:ConvertLoadBalancerBillingType",
        "clb:RenewLoadBalancer",
        "clb:UntagResources",
        "clb:TagResources",
        "clb:ModifyLoadBalancerAttributes"
      ]
    }
  },
  "primaryIdentifier": [
    "/properties/LoadBalancerId"
  ],
  "properties": {
    "AccessLog": {
      "$ref": "#/definitions/AccessLog",
      "description": "负载均衡实例中访问日志的信息",
      "type": "object"
    },
    "AccountId": {
      "description": "负载均衡实例所属的账号ID",
      "type": "string"
    },
    "AddressIpVersion": {
      "description": "负载均衡实例的IP地址类型",
      "type": "string"
    },
    "AllowedPorts": {
      "description": "允许的多个端口范围",
      "insertionOrder": false,
      "items": {
        "type": "string"
      },
      "type": "array",
      "uniqueItems": true
    },
    "BusinessStatus": {
      "description": "负载均衡实例是否被锁定",
      "type": "string"
    },
    "BypassSecurityGroupEnabled": {
      "description": "是否开启bypass安全组功能",
      "type": "string"
    },
    "CreateTime": {
      "description": "负载均衡实例的创建时间",
      "semantic": "createdTime",
      "type": "string"
    },
    "DeletedTime": {
      "description": "负载均衡实例的预期回收时间",
      "type": "string"
    },
    "Description": {
      "description": "负载均衡实例的描述",
      "semantic": "description",
      "type": "string"
    },
    "Eip": {
      "$ref": "#/definitions/Eip",
      "description": "负载均衡实例的公网IP的信息",
      "type": "object"
    },
    "EipAddress": {
      "description": "公网IP地址",
      "semantic": "publicIp",
      "type": "string"
    },
    "EipID": {
      "description": "公网IP ID",
      "type": "string"
    },
    "Enabled": {
      "description": "负载均衡实例的启用状态",
      "type": "boolean"
    },
    "Eni": {
      "$ref": "#/definitions/Eni",
      "description": "负载均衡实例的私网IPv4地址信息",
      "type": "object"
    },
    "EniAddressNum": {
      "description": "负载均衡实例的私网IPv4地址数量。该参数和参数EniAddress不能同时传入，如果传入该参数，则无需传入EniAddress",
      "type": "number"
    },
    "EniIpv6Address": {
      "description": "负载均衡实例的私网IPv6地址",
      "type": "string"
    },
    "Enis": {
      "$ref": "#/definitions/Eni",
      "description": "负载均衡实例的私网IPv4地址信息列表，创建时通过EniAddressNum指定数量",
      "type": "object"
    },
    "ExclusiveClusterId": {
      "description": "独占集群Id",
      "type": "string"
    },
    "ExpiredTime": {
      "description": "负载均衡实例到期时间",
      "type": "string"
    },
    "Listeners": {
      "description": "负载均衡实例中监听器的信息",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/Listener"
      },
      "type": "array",
      "uniqueItems": true
    },
    "LoadBalancerBillingType": {
      "description": "负载均衡实例计费方式",
      "type": "number"
    },
    "LoadBalancerId": {
      "description": "负载均衡实例ID",
      "semantic": "resourceId",
      "type": "string"
    },
    "LoadBalancerName": {
      "description": "负载均衡实例的名称",
      "semantic": "resourceName",
      "type": "string"
    },
    "LoadBalancerSpec": {
      "description": "负载均衡实例的规格",
      "type": "string"
    },
    "LockReason": {
      "description": "负载均衡实例被冻结的原因",
      "type": "string"
    },
    "MasterZoneId": {
      "description": "负载均衡实例的主可用区ID",
      "type": "string"
    },
    "ModificationProtectionReason": {
      "description": "设置修改保护状态的原因",
      "type": "string"
    },
    "ModificationProtectionStatus": {
      "description": "负载均衡实例在控制台上修改保护的状态",
      "type": "string"
    },
    "NewArch": {
      "description": "是否为新架构",
      "type": "boolean"
    },
    "OrderId": {
      "description": "订单ID。仅在创建动作的参数LoadBalancerBillingType配置为1时，改参数有值",
      "type": "string"
    },
    "OverdueTime": {
      "description": "负载均衡实例的冻结时间",
      "type": "string"
    },
    "Period": {
      "description": "包年包月计费类型的时长数量",
      "type": "number"
    },
    "PeriodUnit": {
      "description": "包年包月计费类型的时长单位",
      "type": "string"
    },
    "ProjectName": {
      "description": "CLB实例所属项目的名称",
      "semantic": "projectName",
      "type": "string"
    },
    "RegionId": {
      "description": "请求的Region",
      "type": "string"
    },
    "ServerGroups": {
      "description": "负载均衡实例中后端服务器组的信息",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/ServerGroup"
      },
      "type": "array",
      "uniqueItems": true
    },
    "ServiceManaged": {
      "description": "CLB实例是否为托管资源",
      "type": "boolean"
    },
    "SlaveZoneId": {
      "description": "负载均衡实例的备可用区ID",
      "type": "string"
    },
    "Status": {
      "description": "负载均衡实例状态",
      "enum": [
        "Provisioning",
        "Creating",
        "Inactive",
        "Active",
        "Configuring",
        "Deleting",
        "CreateFailed"
      ],
      "semantic": "status",
      "type": "string"
    },
    "SubnetId": {
      "description": "负载均衡实例所属VPC内的子网ID",
      "type": "string"
    },
    "Tags": {
      "description": "CLB实例标签",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/Tag"
      },
      "semantic": "tags",
      "type": "array",
      "uniqueItems": true
    },
    "TimestampRemoveEnabled": {
      "description": "是否开启 TCP Timestamp 清除功能",
      "type": "string"
    },
    "Type": {
      "description": "负载均衡实例的类型",
      "type": "string"
    },
    "UpdateTime": {
      "description": "负载均衡实例的最近操作时间",
      "semantic": "updatedTime",
      "type": "string"
    },
    "VpcId": {
      "description": "负载均衡实例所属的VPC ID",
      "type": "string"
    },
    "ZoneType": {
      "description": "负载均衡实例的可用区类型",
      "type": "string"
    }
  },
  "readOnlyProperties": [
    "/properties/LoadBalancerId",
    "/properties/AccountId",
    "/properties/Eni",
    "/properties/Enabled",
    "/properties/BusinessStatus",
    "/properties/Status",
    "/properties/OverdueTime",
    "/properties/LockReason",
    "/properties/DeletedTime",
    "/properties/ExpiredTime",
    "/properties/ServiceManaged",
    "/properties/CreateTime",
    "/properties/UpdateTime",
    "/properties/TimestampRemoveEnabled",
    "/properties/EniIpv6Address"
  ],
  "tagging": {
    "tagOnCreate": true,
    "tagUpdatable": true,
    "taggable": true
  },
  "typeName": "Volcengine::CLB::CLB"
}