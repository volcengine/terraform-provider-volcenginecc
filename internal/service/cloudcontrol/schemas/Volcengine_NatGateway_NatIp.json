{
  "additionalProperties": false,
  "createOnlyProperties": [
    "/properties/NatGatewayId",
    "/properties/NatIp"
  ],
  "description": "从私网NAT网关所属子网分配，用于IP地址转换。",
  "handlers": {
    "create": {
      "permissions": [
        "natgateway:CreateNatIp",
        "natgateway:DescribeNatIpAttributes"
      ]
    },
    "delete": {
      "permissions": [
        "natgateway:DeleteNatIp",
        "natgateway:DescribeNatIpAttributes"
      ]
    },
    "read": {
      "permissions": [
        "natgateway:DescribeNatIpAttributes"
      ]
    },
    "update": {
      "permissions": [
        "natgateway:ModifyNatIpAttributes",
        "natgateway:DescribeNatIpAttributes",
        "natgateway:DescribeNatGatewayAttributes"
      ]
    }
  },
  "primaryIdentifier": [
    "/properties/NatIpId"
  ],
  "properties": {
    "IsDefault": {
      "description": "是否为默认中转IP。true：是。false：否。",
      "type": "boolean"
    },
    "NatGatewayId": {
      "description": "私网NAT网关ID。",
      "type": "string"
    },
    "NatIp": {
      "description": "中转IP的地址。若不填，则系统自动从私网NAT网关所在子网随机分配一个空闲的IP地址。",
      "type": "string"
    },
    "NatIpDescription": {
      "description": "中转IP的描述。长度限制为0~ 255个字符。不填默认为空字符串。需要以字母、中文或数字开头。可包含英文逗号（,）、点号（.）、下划线（_）、空格（ ）、等号（=）、短横线（-）、中文逗号（，）、中文句号（。）。",
      "semantic": "description",
      "type": "string"
    },
    "NatIpId": {
      "description": "中转IP的ID。",
      "semantic": "resourceId",
      "type": "string"
    },
    "NatIpName": {
      "description": "中转IP的名称。长度限制为1 ~ 128个字符。需要以字母、中文或数字开头，可包含点号（.）、下划线（_）和短划线（-）。不填默认为中转IP的ID。",
      "semantic": "resourceName",
      "type": "string"
    },
    "Status": {
      "description": "中转IP的状态。Creating：创建中。Available：可用。Deleting：删除中。Deleted：已删除。",
      "semantic": "status",
      "type": "string"
    },
    "UsingStatus": {
      "description": "中转IP的使用状态。Idle：未使用。UsedBySnat：被SNAT规则使用。UsedByDnat：被DNAT规则使用。UsedByNat：被SNAT/DNAT规则同时使用。",
      "enum": [
        "Idle",
        "UsedBySnat",
        "UsedByDnat",
        "UsedByNat"
      ],
      "type": "string"
    }
  },
  "readOnlyProperties": [
    "/properties/IsDefault",
    "/properties/NatIpId",
    "/properties/Status",
    "/properties/UsingStatus"
  ],
  "required": [
    "NatGatewayId"
  ],
  "tagging": {
    "taggable": false
  },
  "typeName": "Volcengine::NatGateway::NatIp"
}