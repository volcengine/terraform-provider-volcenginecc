{
  "additionalProperties": false,
  "description": "解析记录是域名与 VPC 内资源信息的映射。",
  "filterProperties": {
    "filterableProperties": [
      "/properties/Host",
      "/properties/Line",
      "/properties/RecordId",
      "/properties/Type",
      "/properties/Value",
      "/properties/ZID"
    ],
    "required": [
      "/properties/ZID"
    ]
  },
  "handlers": {
    "create": {
      "permissions": [
        "private_zone:CreateRecord",
        "private_zone:UpdateRecord",
        "private_zone:QueryRecord"
      ]
    },
    "delete": {
      "permissions": [
        "private_zone:DeleteRecord",
        "private_zone:QueryRecord"
      ]
    },
    "list": {
      "permissions": [
        "private_zone:ListRecords"
      ]
    },
    "read": {
      "permissions": [
        "private_zone:QueryRecord"
      ]
    },
    "update": {
      "permissions": [
        "private_zone:UpdateRecord",
        "private_zone:QueryRecord"
      ]
    }
  },
  "primaryIdentifier": [
    "/properties/RecordId"
  ],
  "properties": {
    "CreatedAt": {
      "description": "解析记录的创建时间。",
      "semantic": "createdTime",
      "type": "string"
    },
    "Enable": {
      "description": "解析记录是否被启用。",
      "type": "boolean"
    },
    "Host": {
      "description": "子域名的域名前缀。",
      "type": "string"
    },
    "LastOperator": {
      "description": "最近一次更新解析记录的账号 ID。",
      "type": "string"
    },
    "Line": {
      "description": "解析记录的线路，只能是默认线路 default。",
      "type": "string"
    },
    "RecordId": {
      "description": "解析记录 ID。",
      "semantic": "resourceId",
      "type": "string"
    },
    "Remark": {
      "description": "备注。支持 UTF-8 字符。不能超过 16 个字符。默认值为空字符串。",
      "semantic": "description",
      "type": "string"
    },
    "TTL": {
      "description": "解析记录的过期时间。单位为秒。默认值为 600。",
      "type": "integer"
    },
    "Type": {
      "description": "解析记录类型。该参数的取值如下：A：A 记录。AAAA：AAAA 记录。CNAME：CNAME 记录。TXT：TXT 记录。MX：MX 记录。PTR：PTR 记录。A：A 记录。AAAA：AAAA 记录。CNAME：CNAME 记录。TXT：TXT 记录。MX：MX 记录。PTR：PTR 记录。",
      "type": "string"
    },
    "UpdatedAt": {
      "description": "解析记录最近一次的更新时间。",
      "semantic": "updatedTime",
      "type": "string"
    },
    "Value": {
      "description": "记录值。",
      "type": "string"
    },
    "Weight": {
      "description": "记录的权重。只有域名开启了负载均衡后，记录值的权重才会生效。",
      "type": "integer"
    },
    "ZID": {
      "description": "域名 ID。",
      "type": "integer"
    }
  },
  "readOnlyProperties": [
    "/properties/RecordId",
    "/properties/CreatedAt",
    "/properties/UpdatedAt",
    "/properties/LastOperator"
  ],
  "required": [
    "Host",
    "Type",
    "Value",
    "ZID"
  ],
  "tagging": {
    "taggable": false
  },
  "typeName": "Volcengine::PrivateZone::Record"
}