{
  "additionalProperties": false,
  "createOnlyProperties": [
    "/properties/ProjectName",
    "/properties/ZoneId",
    "/properties/VpcId",
    "/properties/SubnetId",
    "/properties/Version"
  ],
  "definitions": {
    "ChargeDetail": {
      "properties": {
        "AutoRenew": {
          "description": "包年包月实例到期后是否自动续费。",
          "type": "boolean"
        },
        "ChargeEndTime": {
          "description": "实例的结束计费时间，时间显示格式为 YYYY-MM-DD'T'HH:MM:SS'Z'。",
          "type": "string"
        },
        "ChargeExpireTime": {
          "description": "包年包月实例的到期时间。时间显示格式为 YYYY-MM-DD'T'HH:MM:SS'Z'。",
          "type": "string"
        },
        "ChargeStartTime": {
          "description": "实例的开始计费时间，时间显示格式为 YYYY-MM-DD'T'HH:MM:SS'Z'。",
          "type": "string"
        },
        "ChargeStatus": {
          "description": "实例的计费状态。包括：Normal：正常，Overdue：按量计费欠费，Expired：包年包月到期。",
          "type": "string"
        },
        "ChargeType": {
          "description": "实例的计费类型。支持的类型包括：PostPaid ：按量付费，PrePaid：包年包月。",
          "type": "string"
        },
        "OverdueReclaimTime": {
          "description": "实例欠费关停后的预计释放时间。时间显示格式为 YYYY-MM-DD'T'HH:MM:SS'Z'。",
          "type": "string"
        },
        "OverdueTime": {
          "description": "实例的欠费关停时间。时间显示格式为 YYYY-MM-DD'T'HH:MM:SS'Z'。",
          "type": "string"
        },
        "Period": {
          "description": "包年包月类型实例的购买时长。PeriodUnit 指定为 Month 时，取值范围为 1~9。PeriodUnit 指定为 Year 时，取值范围为 1~3。",
          "type": "integer"
        },
        "PeriodUnit": {
          "description": "购买时长的单位，取值如下：MONTHLY：按月购买。YEARLY：按年购买。",
          "type": "string"
        }
      },
      "type": "object"
    },
    "Endpoint": {
      "properties": {
        "EndpointType": {
          "description": "实例的接入点类型。RabbitMQ实例提供的接入点类型包括：WEB：Web UI 接入点，AMQP0_9_1：AMQP 接入点，MQTT：MQTT接入点，WEB_MQTT：WEB ，MQTT 接入点，STOMP：STOMP 接入点。",
          "type": "string"
        },
        "InternalEndpoint": {
          "description": "实例的私网访问域名。",
          "type": "string"
        },
        "InternalIpEndpoint": {
          "description": "公共服务区 IP 接入点。",
          "type": "string"
        },
        "NetworkType": {
          "description": "接入点的网络类型。PrivateNetwork 表示私有网络 VPC。",
          "type": "string"
        },
        "PublicEndpoint": {
          "description": "实例公网访问域名。仅在开启公网访问之后显示。",
          "type": "string"
        }
      },
      "type": "object"
    },
    "Tag": {
      "properties": {
        "Key": {
          "description": "标签的键。",
          "type": "string"
        },
        "Value": {
          "description": "标签的值。",
          "type": "string"
        }
      },
      "required": [
        "Key",
        "Value"
      ],
      "type": "object"
    }
  },
  "description": "消息队列 RabbitMQ版是一款支持 AMQP（Advanced Message Queuing Protocol） 协议的分布式高可用消息队列产品，完全兼容 RabbitMQ 开源生态及多语言客户端，提供丰富的消息特性和灵活的消息路由，同时具备分布式、高吞吐、低延迟、灵活扩展的云消息服务优势，无需用户部署运维，能够轻松实现快速上云。",
  "handlers": {
    "create": {
      "permissions": [
        "rabbitmq:CreateInstance",
        "rabbitmq:DescribeInstanceDetail"
      ]
    },
    "delete": {
      "permissions": [
        "rabbitmq:DeleteInstance",
        "rabbitmq:DescribeInstanceDetail"
      ]
    },
    "list": {
      "permissions": [
        "rabbitmq:DescribeInstances"
      ]
    },
    "read": {
      "permissions": [
        "rabbitmq:DescribeInstanceDetail"
      ]
    },
    "update": {
      "permissions": [
        "rabbitmq:ModifyUserPassword",
        "rabbitmq:ModifyInstanceAttributes",
        "rabbitmq:AddTagsToResource",
        "rabbitmq:RemoveTagsFromResource",
        "rabbitmq:ModifyInstanceChargeType",
        "rabbitmq:ModifyInstanceSpec",
        "rabbitmq:DescribeInstanceDetail"
      ]
    }
  },
  "primaryIdentifier": [
    "/properties/InstanceId"
  ],
  "properties": {
    "AccountId": {
      "description": "创建实例的主账号ID。",
      "type": "string"
    },
    "ApplyPrivateDNSToPublic": {
      "description": "是否已开启公网解析功能。true：已开启, false：已关闭",
      "type": "boolean"
    },
    "ArchType": {
      "description": "实例的类型，即集群版或单机版。",
      "type": "string"
    },
    "ChargeDetail": {
      "$ref": "#/definitions/ChargeDetail",
      "description": "实例的计费方式等计费信息。",
      "type": "object"
    },
    "ComputeSpec": {
      "description": "RabbitMQ实例的计算规格。",
      "type": "string"
    },
    "CreatedTime": {
      "description": "实例的创建时间。",
      "semantic": "createdTime",
      "type": "string"
    },
    "EipId": {
      "description": "EIP的ID。",
      "type": "string"
    },
    "Endpoints": {
      "description": "实例的连接信息。",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/Endpoint"
      },
      "type": "array",
      "uniqueItems": true
    },
    "InstanceDescription": {
      "description": "实例的简单描述。",
      "semantic": "description",
      "type": "string"
    },
    "InstanceId": {
      "description": "RabbitMQ 实例 ID。",
      "semantic": "resourceId",
      "type": "string"
    },
    "InstanceName": {
      "description": "RabbitMQ 实例名称。",
      "semantic": "resourceName",
      "type": "string"
    },
    "InstanceStatus": {
      "description": "实例状态。",
      "semantic": "status",
      "type": "string"
    },
    "IsEncrypted": {
      "description": "是否开启了云盘加密。",
      "type": "boolean"
    },
    "ProjectName": {
      "description": "实例所属的IAM项目。",
      "semantic": "projectName",
      "type": "string"
    },
    "StorageSpace": {
      "description": "实例总存储空间。单位为 GiB。",
      "type": "integer"
    },
    "SubnetId": {
      "description": "VPC的子网ID。",
      "type": "string"
    },
    "Tags": {
      "description": "实例绑定的标签。",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/Tag"
      },
      "semantic": "tags",
      "type": "array",
      "uniqueItems": true
    },
    "UsedStorageSpace": {
      "description": "实例已用存储空间。单位为 GiB。",
      "type": "integer"
    },
    "UserName": {
      "description": "RabbitMQ WebUI管理员账号名。",
      "type": "string"
    },
    "UserPassword": {
      "description": "RabbitMQ WebUI管理员密码。",
      "type": "string"
    },
    "Version": {
      "description": "支持的 RabbitMQ 版本。当前支持的版本包括：3.8.18：RabbitMQ 3.8.18 版本。",
      "type": "string"
    },
    "VpcId": {
      "description": "私有网络（VPC）ID。",
      "type": "string"
    },
    "ZoneDescription": {
      "description": "可用区的描述信息。",
      "type": "string"
    },
    "ZoneId": {
      "description": "实例所在的可用区 ID。对于跨 AZ 的高可用实例，此处会返回多个可用区 ID。",
      "type": "string"
    }
  },
  "readOnlyProperties": [
    "/properties/InstanceId",
    "/properties/InstanceStatus",
    "/properties/UsedStorageSpace",
    "/properties/ZoneDescription",
    "/properties/ApplyPrivateDNSToPublic",
    "/properties/ChargeDetail/ChargeStatus",
    "/properties/ChargeDetail/ChargeStartTime",
    "/properties/ChargeDetail/OverdueReclaimTime",
    "/properties/ChargeDetail/OverdueTime",
    "/properties/ChargeDetail/ChargeExpireTime",
    "/properties/ChargeDetail/ChargeEndTime",
    "/properties/IsEncrypted",
    "/properties/AccountId",
    "/properties/CreatedTime",
    "/properties/ArchType",
    "/properties/Endpoints"
  ],
  "required": [
    "ComputeSpec",
    "StorageSpace",
    "VpcId",
    "SubnetId",
    "UserName",
    "UserPassword",
    "Version",
    "ZoneId"
  ],
  "tagging": {
    "tagOnCreate": true,
    "tagUpdatable": true,
    "taggable": true
  },
  "typeName": "Volcengine::RabbitMQ::Instance",
  "writeOnlyProperties": [
    "/properties/UserPassword"
  ]
}