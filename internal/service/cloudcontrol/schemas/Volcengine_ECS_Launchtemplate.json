{
  "additionalProperties": false,
  "createOnlyProperties": [
    "/properties/LaunchTemplateName",
    "/properties/LaunchTemplateProjectName",
    "/properties/LaunchTemplateTags",
    "/properties/LaunchTemplateVersion"
  ],
  "definitions": {
    "Eip": {
      "properties": {
        "Bandwidth": {
          "description": "公网IP的带宽上限，单位：Mbps。取值范围：1～200。默认值：1。",
          "maximum": 200,
          "type": "integer"
        },
        "BandwidthPackageId": {
          "description": "共享带宽包的ID，表示将公网IP加入到共享带宽包。",
          "type": "string"
        },
        "BillingType": {
          "description": "公网IP的计费方式，取值：2：按量计费-按带宽上限计费。3：按量计费-按实际流量计费。",
          "type": "integer"
        },
        "ISP": {
          "description": "公网IP的线路类型，取值：BGP：BGP（多线）。ChinaMobile：中国移动静态单线。ChinaTelecom：国电信静态单线。ChinaUnicom：中国联通静态单线。",
          "type": "string"
        },
        "ReleaseWithInstance": {
          "description": "公网IP是否随实例删除，仅按量计费公网IP生效。",
          "type": "boolean"
        },
        "SecurityProtectionInstanceId": {
          "description": "安全防护包ID。",
          "type": "integer"
        },
        "SecurityProtectionTypes": {
          "description": "公网IP的安全防护类型。取值：AntiDDoS_Enhanced：申请增强防护类型的公网IP，可加入到DDoS原生防护（企业版）实例。",
          "insertionOrder": false,
          "items": {
            "type": "string"
          },
          "type": "array",
          "uniqueItems": true
        }
      },
      "type": "object"
    },
    "LaunchTemplateVersion": {
      "properties": {
        "DeploymentSetGroupNumber": {
          "description": "当ECS实例要加入或调整的部署集策略为部署集组高可用策略（AvailabilityGroup）时，可以通过该参数指定实例在部署集中的分组号。",
          "type": "integer"
        },
        "DeploymentSetId": {
          "description": "实例需要加入的部署集ID。",
          "type": "string"
        },
        "Description": {
          "description": "实例的描述。",
          "maxLength": 255,
          "type": "string"
        },
        "Eip": {
          "$ref": "#/definitions/Eip",
          "description": "实例绑定的公网IP信息。",
          "type": "object"
        },
        "HostName": {
          "description": "实例的主机名。Linux系统的主机名长度限制为2～59个字符。Windows系统的主机名长度限制为2～10个字符。",
          "type": "string"
        },
        "HpcClusterId": {
          "description": "当创建高性能计算GPU型实例时，请指定高性能计算集群ID。",
          "type": "string"
        },
        "ImageId": {
          "description": "镜像ID。",
          "type": "string"
        },
        "ImageName": {
          "description": "镜像名称。",
          "type": "string"
        },
        "InstanceChargeType": {
          "default": "PostPaid",
          "description": "实例和云盘的计费类型，取值：PostPaid：按量计费。PrePaid：包年包月。Esi：弹性预约实例。Segmented：时段型弹性预约实例。",
          "type": "string"
        },
        "InstanceName": {
          "description": "实例的名称。",
          "maxLength": 128,
          "type": "string"
        },
        "InstanceTypeId": {
          "description": "实例的规格。",
          "type": "string"
        },
        "KeepImageCredential": {
          "description": "是否保留镜像设置，取值：true：保留镜像设置，保留后将使用镜像预设的密码或密钥对登录实例。false（默认）：不保留镜像设置。",
          "type": "boolean"
        },
        "KeyPairName": {
          "description": "实例绑定的密钥对。",
          "type": "string"
        },
        "NetworkInterfaces": {
          "description": "实例挂载的网卡信息。",
          "insertionOrder": false,
          "items": {
            "$ref": "#/definitions/NetworkInterface"
          },
          "type": "array",
          "uniqueItems": true
        },
        "ProjectName": {
          "description": "实例所属项目。",
          "type": "string"
        },
        "ScheduledInstance": {
          "$ref": "#/definitions/ScheduledInstance",
          "description": "弹性预约单信息。",
          "type": "object"
        },
        "SecurityEnhancementStrategy": {
          "description": "是否开启安全加固。Active:开启安全加固，仅对公共镜像生效。InActive:关闭安全加固，对所有镜像生效。",
          "enum": [
            "Active",
            "InActive"
          ],
          "type": "string"
        },
        "SpotPriceLimit": {
          "description": "抢占式实例的每小时最高价格。",
          "type": "number"
        },
        "SpotStrategy": {
          "description": "按量计费的抢占式策略。取值：NoSpot：表示创建正常按量计费实例。SpotAsPriceGo：系统自动出价，跟随当前市场实际价格的抢占式实例。SpotWithPriceLimit：设置出价上限的抢占式实例。",
          "type": "string"
        },
        "SuffixIndex": {
          "default": 1,
          "description": "有序后缀的起始序号。",
          "maximum": 999,
          "minimum": 1,
          "type": "integer"
        },
        "Tags": {
          "description": "实例的标签信息。",
          "insertionOrder": false,
          "items": {
            "$ref": "#/definitions/Tag"
          },
          "type": "array",
          "uniqueItems": true
        },
        "UniqueSuffix": {
          "default": false,
          "description": "表示当创建多台实例时，是否为Hostname和InstanceName自动添加有序后缀。",
          "type": "boolean"
        },
        "UserData": {
          "description": "实例的自定义数据。",
          "type": "string"
        },
        "VersionDescription": {
          "description": "模版版本描述。",
          "type": "string"
        },
        "Volumes": {
          "description": "实例绑定的云盘信息。",
          "insertionOrder": false,
          "items": {
            "$ref": "#/definitions/Volume"
          },
          "type": "array",
          "uniqueItems": true
        },
        "VpcId": {
          "description": "私有网络ID。",
          "type": "string"
        },
        "ZoneId": {
          "description": "实例所属可用区ID。",
          "type": "string"
        }
      },
      "type": "object"
    },
    "NetworkInterface": {
      "properties": {
        "SecurityGroupIds": {
          "description": "网卡关联的安全组ID。",
          "insertionOrder": false,
          "items": {
            "type": "string"
          },
          "type": "array",
          "uniqueItems": true
        },
        "SubnetId": {
          "description": "实例的私有网络子网ID。",
          "type": "string"
        }
      },
      "type": "object"
    },
    "ScheduledInstance": {
      "description": "弹性预约单信息。",
      "properties": {
        "ScheduledInstanceDescription": {
          "description": "弹性预约单的描述。",
          "type": "string"
        },
        "ScheduledInstanceName": {
          "description": "弹性预约单的名称。",
          "type": "string"
        }
      },
      "type": "object"
    },
    "Tag": {
      "properties": {
        "Key": {
          "description": "给资源添加的用户标签的标签键。",
          "type": "string"
        },
        "Value": {
          "description": "给资源添加的用户标签的标签值。",
          "type": "string"
        }
      },
      "required": [
        "Key"
      ],
      "type": "object"
    },
    "Volume": {
      "properties": {
        "DeleteWithInstance": {
          "description": "云盘是否随实例释放。",
          "type": "boolean"
        },
        "ExtraPerformanceIOPS": {
          "description": "云盘额外IOPS性能大小，仅ESSD FlexPL数据盘支持，单位为次/秒。 ExtraPerformanceTypeId为Balance或IOPS时需设置该参数。各个类型的额外性能取值范围如下：IOPS型：1～50000。Balance型：1～50000。",
          "type": "integer"
        },
        "ExtraPerformanceThroughputMB": {
          "description": "云盘额外吞吐性能大小，单位MB/s，仅ESSD FlexPL数据盘支持。 ExtraPerformanceTypeId为Throughput时需设置该参数。 取值范围：1～650。",
          "type": "integer"
        },
        "ExtraPerformanceTypeId": {
          "description": "云盘额外性能的类型，仅ESSD FlexPL数据盘支持。关于额外性能的更多介绍，请参见云盘额外性能。取值：Balance：均衡型额外性能。IOPS：IOPS型额外性能。Throughput：吞吐量型额外性能。",
          "type": "string"
        },
        "Size": {
          "description": "云盘大小，单位为GB。系统盘：ESSD_PL0：20~2048 PTSSD：10~500。数据盘：ESSD_PL0：10~32768 PTSSD：20~8192",
          "type": "integer"
        },
        "SnapshotId": {
          "description": "使用快照创建云盘，仅支持创建数据盘。您可以调用DescribeSnapshots接口查询快照ID。",
          "type": "string"
        },
        "VolumeType": {
          "description": "云盘类型。ESSD_PL0：极速型SSD PL0。PTSSD：性能型SSD。",
          "type": "string"
        }
      },
      "type": "object"
    }
  },
  "description": "实例启动模板是一系列实例配置信息的集合，用于快速购买实例。在模板中，您可以存储除密码外的全部配置信息，包括密钥对、计算规格、云盘、网络等，如果配置信息均有效，便可以一键成功购买实例，无需进行重复配置。",
  "filterProperties": {
    "filterableProperties": [
      "/properties/LaunchTemplateId",
      "/properties/LaunchTemplateName",
      "/properties/LaunchTemplateProjectName",
      "/properties/LaunchTemplateTags/*/Key",
      "/properties/LaunchTemplateTags/*/Value"
    ]
  },
  "handlers": {
    "create": {
      "permissions": [
        "ecs:CreateLaunchTemplate",
        "ecs:DescribeLaunchTemplates"
      ]
    },
    "delete": {
      "permissions": [
        "ecs:DeleteLaunchTemplate",
        "ecs:DescribeLaunchTemplates"
      ]
    },
    "list": {
      "permissions": [
        "ecs:DescribeLaunchTemplates"
      ]
    },
    "read": {
      "permissions": [
        "ecs:DescribeLaunchTemplates"
      ]
    },
    "update": {
      "permissions": [
        "ecs:ModifyTemplateDefaultVersion",
        "ecs:DescribeLaunchTemplates"
      ]
    }
  },
  "primaryIdentifier": [
    "/properties/LaunchTemplateId"
  ],
  "properties": {
    "CreatedAt": {
      "description": "实例启动模板的创建时间。",
      "semantic": "createdTime",
      "type": "string"
    },
    "DefaultVersionNumber": {
      "description": "实例启动模板的默认版本号。",
      "type": "integer"
    },
    "LatestVersionNumber": {
      "description": "实例启动模的最新版本号。",
      "type": "integer"
    },
    "LaunchTemplateId": {
      "description": "实例启动模板 ID。",
      "semantic": "resourceId",
      "type": "string"
    },
    "LaunchTemplateName": {
      "description": "实例启动模板名称。",
      "maxLength": 128,
      "semantic": "resourceName",
      "type": "string"
    },
    "LaunchTemplateProjectName": {
      "description": "实例启动模版所属项目。",
      "semantic": "projectName",
      "type": "string"
    },
    "LaunchTemplateTags": {
      "description": "实例启动模版的标签信息。",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/Tag"
      },
      "semantic": "tags",
      "type": "array",
      "uniqueItems": true
    },
    "LaunchTemplateVersion": {
      "$ref": "#/definitions/LaunchTemplateVersion",
      "description": "实例启动模板版本信息。",
      "type": "object"
    },
    "UpdatedAt": {
      "description": "实例启动模板的更新时间。",
      "semantic": "updatedTime",
      "type": "string"
    }
  },
  "readOnlyProperties": [
    "/properties/LaunchTemplateId",
    "/properties/LatestVersionNumber",
    "/properties/CreatedAt",
    "/properties/UpdatedAt"
  ],
  "required": [
    "LaunchTemplateName"
  ],
  "tagging": {
    "tagOnCreate": true,
    "taggable": true
  },
  "typeName": "Volcengine::ECS::Launchtemplate",
  "writeOnlyProperties": [
    "/properties/LaunchTemplateVersion"
  ]
}