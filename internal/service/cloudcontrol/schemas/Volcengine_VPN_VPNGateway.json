{
  "additionalProperties": false,
  "createOnlyProperties": [
    "/properties/BillingType",
    "/properties/DualTunnelEnabled",
    "/properties/IpStackType",
    "/properties/IpVersion",
    "/properties/Period",
    "/properties/PeriodUnit",
    "/properties/ProjectName",
    "/properties/SecondarySubnetId",
    "/properties/SubnetId",
    "/properties/Tags",
    "/properties/VpcId"
  ],
  "definitions": {
    "Tag": {
      "description": "VPN网关的标签信息。",
      "properties": {
        "Key": {
          "description": "VPN网关标签的标签键（Key）。参数 - N：表示标签键的序号，取值范围：1～20。多个标签键之间用\u0026分隔。命名规则如下：不能以volc:或sys:的任意大小写组合开头。只能包含语言字符、数字、空格和英文符号“_”、“.”、“:”、“/”、“=”、“+”、“-”、“@”。长度限制在1～128个字符之间。说明同一资源的标签键不允许重复。",
          "type": "string"
        },
        "Value": {
          "description": "VPN网关标签的标签值（Value）。参数 - N：表示标签值的序号，取值范围：1～20。多个标签值之间用\u0026分隔。命名规则如下：只能包含语言字符、数字、空格和英文符号“_”、“.”、“:”、“/”、“=”、“+”、“-”、“@”。允许为空，长度限制在0～256个字符之间。大小写敏感，不能以空格开头或结尾。说明传入Tags.N.Value则必须传入Tags.N.Key。",
          "type": "string"
        }
      },
      "required": [
        "Key"
      ],
      "type": "object"
    }
  },
  "description": "当您需要通过VPN连接实现私有网络与本地数据中心或其他私有网络之间的通信，您需要先创建一个VPN网关。",
  "filterProperties": {
    "filterableProperties": [
      "/properties/VpnGatewayId",
      "/properties/VpnGatewayName",
      "/properties/VpcId",
      "/properties/SubnetId",
      "/properties/IpAddress",
      "/properties/ProjectName",
      "/properties/IpsecEnabled",
      "/properties/SslEnabled",
      "/properties/IpVersion",
      "/properties/IpStackType",
      "/properties/DualTunnelEnabled",
      "/properties/Tags/*/Key",
      "/properties/Tags/*/Value"
    ]
  },
  "handlers": {
    "create": {
      "permissions": [
        "vpn:CreateVpnGateway",
        "vpn:DescribeVpnGatewayAttributes"
      ]
    },
    "delete": {
      "permissions": [
        "vpn:DeleteVpnGateway",
        "vpn:DescribeVpnGatewayAttributes"
      ]
    },
    "list": {
      "permissions": [
        "vpn:DescribeVpnGateways"
      ]
    },
    "read": {
      "permissions": [
        "vpn:DescribeVpnGatewayAttributes"
      ]
    },
    "update": {
      "permissions": [
        "vpn:ModifyVpnGatewayAttributes",
        "vpn:DescribeVpnGatewayAttributes"
      ]
    }
  },
  "primaryIdentifier": [
    "/properties/VpnGatewayId"
  ],
  "properties": {
    "Asn": {
      "description": "VPN网关的ASN。",
      "format": "int64",
      "type": "integer"
    },
    "Bandwidth": {
      "description": "VPN网关的带宽规格。",
      "format": "int64",
      "type": "integer"
    },
    "BillingType": {
      "description": "VPN网关的计费类型。1：包年包月。2：按量计费（按固定规格）",
      "format": "int64",
      "type": "integer"
    },
    "BusinessStatus": {
      "description": "VPN网关的计费状态。Normal: 正常。FinancialLocked: 被锁定。",
      "type": "string"
    },
    "ConnectionCount": {
      "description": "VPN网关关联IPsec连接的数量。",
      "format": "int64",
      "type": "integer"
    },
    "CreateTime": {
      "description": "创建VPN网关的时间。",
      "type": "string"
    },
    "DeletedTime": {
      "description": "预期资源强制回收时间。",
      "type": "string"
    },
    "Description": {
      "description": "VPN网关的描述。",
      "type": "string"
    },
    "DualTunnelEnabled": {
      "description": "VPN网关是否开启双隧道模式。true：开启false：关闭",
      "type": "boolean"
    },
    "ExpiredTime": {
      "description": "VPN网关的到期时间。",
      "type": "string"
    },
    "IpAddress": {
      "description": "VPN网关的公网IP地址。",
      "type": "string"
    },
    "IpStackType": {
      "description": "VPN 网关的协议类型。ipv4_only：IPv4协议类型。ipv6_only：IPv6协议类型。ipv4_ipv6：双栈协议类型。",
      "type": "string"
    },
    "IpVersion": {
      "description": "VPN网关的IP地址类型。ipv4：IPv4地址类型。ipv6：IPv6地址类型。",
      "type": "string"
    },
    "IpsecEnabled": {
      "description": "是否开启IPSec功能。true：开启false：关闭。",
      "type": "boolean"
    },
    "IsBlocked": {
      "description": "当前资源的状态是否被封禁。true：封禁false：未封禁。",
      "type": "boolean"
    },
    "LockReason": {
      "description": "VPN网关的锁定原因。financial：因欠费被锁定。security：因安全被锁定。unlock：未锁定。",
      "type": "string"
    },
    "OverdueTime": {
      "description": "资源冻结时间。仅当资源因为欠费冻结，此参数才会有返回值，否则均返回空值。",
      "type": "string"
    },
    "Period": {
      "description": "包年包月类型VPN网关购买的时长，默认“1”。当PeriodUnit传入值为Month，Period取值范围：1~9、12、24、36。当PeriodUnit传入值为Year，Period取值范围：1～3。",
      "format": "int64",
      "type": "integer"
    },
    "PeriodUnit": {
      "description": "包年包月类型VPN网关购买时长的单位，默认“Month”。Month：月Year：年",
      "type": "string"
    },
    "ProjectName": {
      "description": "VPN网关所属的项目。不填默认为default，即创建的资源属于default项目。您可调用ListProjects查询当前账号下项目的信息。",
      "type": "string"
    },
    "RouteCount": {
      "description": "VPN网关的路由条目数量。",
      "format": "int64",
      "type": "integer"
    },
    "SecondaryIpAddress": {
      "description": "VPN网关的第二个公网IP地址。仅双隧道模式的VPN网关实例返回该参数，单隧道模式的VPN网关实例不返回该参数。",
      "type": "string"
    },
    "SecondarySubnetId": {
      "description": "VPN网关实例关联的第二个子网的ID。仅双隧道模式的VPN网关实例返回该参数，单隧道模式的VPN网关实例不返回该参数。",
      "type": "string"
    },
    "SslEnabled": {
      "description": "是否开启SSL VPN功能。true：开启false：关闭",
      "type": "boolean"
    },
    "SslMaxConnections": {
      "description": "VPN网关的最大SSL连接数。",
      "format": "int64",
      "type": "integer"
    },
    "Status": {
      "description": "VPN网关的状态。Creating: 创建中Deleting: 删除中Pending：配置中Available：可用",
      "type": "string"
    },
    "SubnetId": {
      "description": "VPN网关使用的端口所属子网的ID。",
      "type": "string"
    },
    "Tags": {
      "description": "VPN网关已添加的全部标签信息。",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/Tag"
      },
      "type": "array",
      "uniqueItems": true
    },
    "UpdateTime": {
      "description": "更新VPN网关的时间。",
      "type": "string"
    },
    "VpcId": {
      "description": "VPN网关所在VPC的ID。",
      "type": "string"
    },
    "VpnGatewayId": {
      "description": "VPN网关的ID。",
      "semantic": "resourceId",
      "type": "string"
    },
    "VpnGatewayName": {
      "description": "VPN网关的名称。",
      "type": "string"
    }
  },
  "readOnlyProperties": [
    "/properties/BusinessStatus",
    "/properties/ConnectionCount",
    "/properties/CreateTime",
    "/properties/DeletedTime",
    "/properties/ExpiredTime",
    "/properties/IpAddress",
    "/properties/IsBlocked",
    "/properties/LockReason",
    "/properties/OverdueTime",
    "/properties/RouteCount",
    "/properties/SecondaryIpAddress",
    "/properties/Status",
    "/properties/UpdateTime",
    "/properties/VpnGatewayId"
  ],
  "required": [
    "Bandwidth",
    "SubnetId",
    "VpcId"
  ],
  "tagging": {
    "tagOnCreate": true,
    "taggable": true
  },
  "typeName": "Volcengine::VPN::VPNGateway",
  "writeOnlyProperties": [
    "/properties/Period",
    "/properties/PeriodUnit"
  ]
}