{
  "additionalProperties": false,
  "createOnlyProperties": [
    "/properties/ProjectName",
    "/properties/SubnetId",
    "/properties/PrimaryIpAddress/PrivateIpAddress"
  ],
  "definitions": {
    "AssociatedElasticIp": {
      "properties": {
        "AllocationId": {
          "description": "公网IP的ID。",
          "type": "string"
        },
        "EipAddress": {
          "description": "公网IP的地址。",
          "semantic": "publicIp",
          "type": "string"
        },
        "ReleaseWithInstance": {
          "description": "公网IP是否随云服务器实例删除。仅主网卡的主私网IP绑定按量计费公网IP有效。开启后，当云服务器实例被系统自动回收（退订24小时后、到期回收、欠费回收）或被调用DeleteInstances接口时，公网IP随其一同释放，true：是，false：否。",
          "type": "boolean"
        }
      },
      "type": "object"
    },
    "PrivateIpSet": {
      "properties": {
        "AssociatedElasticIp": {
          "$ref": "#/definitions/AssociatedElasticIp",
          "description": "网卡主私网IPv4关联的公网IP的信息。"
        },
        "Primary": {
          "description": "是否为主私网IPv4地址。",
          "type": "boolean"
        },
        "PrivateIpAddress": {
          "description": "网卡的私网IP地址。",
          "semantic": "privateIp",
          "type": "string"
        }
      },
      "type": "object"
    },
    "Tag": {
      "properties": {
        "Key": {
          "description": "用户标签的标签键。",
          "type": "string"
        },
        "Value": {
          "description": "用户标签的标签值。",
          "type": "string"
        }
      },
      "required": [
        "Key"
      ],
      "type": "object"
    }
  },
  "description": "网卡即虚拟网卡。在私有网络里，网卡是挂载在云服务器、负载均衡等云资源上的一种网络接口。每张网卡均有一个主私网IP地址。若有需要，可手动为其分配多个辅助私网IP地址。",
  "handlers": {
    "create": {
      "permissions": [
        "vpc:CreateNetworkInterface",
        "vpc:DescribeNetworkInterfaceAttributes"
      ]
    },
    "delete": {
      "permissions": [
        "vpc:DeleteNetworkInterface",
        "vpc:DetachNetworkInterface",
        "vpc:DescribeNetworkInterfaces"
      ]
    },
    "list": {
      "permissions": [
        "vpc:DescribeNetworkInterfaces"
      ]
    },
    "read": {
      "permissions": [
        "vpc:DescribeNetworkInterfaceAttributes"
      ]
    },
    "update": {
      "permissions": [
        "vpc:ModifyNetworkInterfaceAttributes",
        "vpc:DetachNetworkInterface",
        "vpc:AttachNetworkInterface",
        "vpc:UnassignIpv6Addresses",
        "vpc:AssignIpv6Addresses",
        "vpc:UnassignPrivateIpAddresses",
        "vpc:AssignPrivateIpAddresses",
        "vpc:AssignIpv6Addresses",
        "vpc:UntagResources",
        "vpc:TagResources",
        "vpc:DescribeNetworkInterfaceAttributes"
      ]
    }
  },
  "primaryIdentifier": [
    "/properties/NetworkInterfaceId"
  ],
  "properties": {
    "AccountId": {
      "description": "网卡所有者ID。",
      "type": "string"
    },
    "CreatedTime": {
      "description": "网卡创建时间。",
      "semantic": "createdTime",
      "type": "string"
    },
    "DeleteOnTermination": {
      "description": "是否开启随云服务器实例删除，true为开启，false为不开启。",
      "type": "boolean"
    },
    "Description": {
      "description": "辅助网卡的描述信息。长度限制为0~ 255个字符，需要以字母、中文或数字开头；可包含英文逗号（,）、点号（.）、下划线（_）、空格（ ）、等号（=）、短横线（-）、中文逗号（，）、中文句号（。），不填默认空字符串。",
      "maxLength": 255,
      "semantic": "description",
      "type": "string"
    },
    "InstanceId": {
      "description": "网卡绑定实例的ID，如果网卡未挂载或参数ServiceManaged为true，则InstanceId返回值均为空。",
      "type": "string"
    },
    "Ipv6AddressCount": {
      "description": "为网卡分配私网IPv6地址的数量，传入该参数，系统会自动从网卡所属子网中，分配相应数量的空闲私网IPv6地址，创建时与IPv6Sets不能同时传入。",
      "format": "int64",
      "type": "integer"
    },
    "Ipv6Sets": {
      "description": "网卡的IPv6地址列表，例如[2408::153:3921:XX:XX:7b12:1c5f, 2408:4008:2cf:XX:XX:dd1e:2a22:5ddf]。",
      "insertionOrder": false,
      "items": {
        "type": "string"
      },
      "type": "array",
      "uniqueItems": true
    },
    "MacAddress": {
      "description": "网卡的MAC地址。",
      "type": "string"
    },
    "NetworkInterfaceId": {
      "description": "网卡ID。",
      "semantic": "resourceId",
      "type": "string"
    },
    "NetworkInterfaceName": {
      "description": "网卡名称，长度限制为1 ~ 128个字符，需要以字母、中文或数字开头，可包含点号（.）、下划线（_）和短横线（-），不填默认是辅助网卡的ID。",
      "maxLength": 128,
      "semantic": "resourceName",
      "type": "string"
    },
    "PortSecurityEnabled": {
      "description": "是否为网卡开启源/目的地址检查，开启后，系统会自动阻止源地址或目的地址不是当前网卡的流量，true为开启，false为不开启（默认值）。",
      "type": "boolean"
    },
    "PrimaryIpAddress": {
      "$ref": "#/definitions/PrivateIpSet",
      "description": "网卡的主私有IPv4地址，不支持修改地址，可以修改公网IP。如果指定，必须是对应子网中的一个空闲的私网IPv4地址，如果不指定，从对应子网中自动分配一个空闲的私网IPv4地址。例如：192.XX.XX.10。",
      "type": "object"
    },
    "PrivateIpSets": {
      "description": "网卡的私网IPv4地址列表。",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/PrivateIpSet"
      },
      "type": "array",
      "uniqueItems": true
    },
    "ProjectName": {
      "description": "网卡所属项目的名称。",
      "semantic": "projectName",
      "type": "string"
    },
    "SecondaryPrivateIpAddressCount": {
      "description": "为辅助网卡自动分配辅助私网IPv4地址数量，取值1~49。创建时不能与PrivateIpSets同时传入。",
      "format": "int64",
      "maximum": 49,
      "type": "integer"
    },
    "SecurityGroupIds": {
      "description": "辅助网卡加入一个或多个安全组的ID。",
      "insertionOrder": false,
      "items": {
        "type": "string"
      },
      "type": "array",
      "uniqueItems": true
    },
    "ServiceManaged": {
      "description": "是否为火山引擎官方服务网卡，true为是，false为否。",
      "type": "boolean"
    },
    "Status": {
      "description": "网卡的绑定状态。",
      "semantic": "status",
      "type": "string"
    },
    "SubnetId": {
      "description": "辅助网卡所在子网的ID。",
      "type": "string"
    },
    "Tags": {
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/Tag",
        "description": "标签。"
      },
      "semantic": "tags",
      "type": "array",
      "uniqueItems": true
    },
    "Type": {
      "description": "网卡类型。primary：主网卡，secondary：辅助网卡",
      "type": "string"
    },
    "UpdatedTime": {
      "description": "更新网卡的时间。",
      "semantic": "updatedTime",
      "type": "string"
    },
    "VpcId": {
      "description": "网卡所属的VPC的ID。",
      "type": "string"
    },
    "VpcName": {
      "description": "网卡所属VPC的名称。",
      "type": "string"
    },
    "ZoneId": {
      "description": "网卡所属可用区的ID。",
      "type": "string"
    }
  },
  "readOnlyProperties": [
    "/properties/AccountId",
    "/properties/MacAddress",
    "/properties/NetworkInterfaceId",
    "/properties/ServiceManaged",
    "/properties/Status",
    "/properties/Type",
    "/properties/UpdatedTime",
    "/properties/CreatedTime",
    "/properties/VpcId",
    "/properties/VpcName",
    "/properties/PrivateIpSets/*/Primary",
    "/properties/PrivateIpSets/*/AssociatedElasticIp/EipAddress",
    "/properties/PrivateIpSets/*/AssociatedElasticIp/ReleaseWithInstance",
    "/properties/PrimaryIpAddress/Primary",
    "/properties/PrimaryIpAddress/AssociatedElasticIp/EipAddress",
    "/properties/PrimaryIpAddress/AssociatedElasticIp/ReleaseWithInstance"
  ],
  "tagging": {
    "tagOnCreate": true,
    "tagUpdatable": true,
    "taggable": true
  },
  "typeName": "Volcengine::VPC::ENI",
  "writeOnlyProperties": [
    "/properties/DeleteOnTermination",
    "/properties/Ipv6AddressCount",
    "/properties/SecondaryPrivateIpAddressCount"
  ]
}