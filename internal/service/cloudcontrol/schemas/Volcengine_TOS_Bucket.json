    "ACL": {
      "description": "桶的访问控制权限。",
      "properties": {
        "BucketACLDelivered": {
          "description": "是否开启对象默认继承桶 ACL 功能。true：开启对象默认继承桶 ACL 功能。false：关闭对象默认继承桶 ACL 功能。",
          "type": "boolean"
        },
        "Grants": {
          "description": "对象的访问控制权限根节点。",
          "insertionOrder": false,
          "items": {
            "$ref": "#/definitions/Grant"
          },
          "type": "array",
          "uniqueItems": true
        },
        "Owner": {
          "$ref": "#/definitions/Owner",
