  "description": "存储桶是存储对象（Object）的容器，所有的对象都必须隶属于某个存储桶。存储桶具有各种配置属性，包括地域、访问权限等。您可以根据实际需求，创建不同类型的存储桶来存储不同的数据。",
  "handlers": {
    "create": {
      "permissions": [
        "tos:CreateBucket",
        "tos:PutBucketACL",
        "tos:PutBucketVersioning",
        "tos:PutBucketTagging",
        "tos:PutBucketLifecycle",
        "tos:HeadBucket",
        "tos:ListBuckets",
        "tos:GetBucketACL",
        "tos:GetBucketVersioning",
        "tos:GetBucketTagging",
        "tos:GetBucketLifecycle",
        "tos:PutBucketPolicy",
        "tos:GetBucketPolicy"
      ]
    },
    "delete": {
      "permissions": [
        "tos:DeleteBucket",
        "tos:HeadBucket"
      ]
    },
    "list": {
      "permissions": [
        "tos:ListBuckets"
      ]
    },
    "read": {
      "permissions": [
        "tos:HeadBucket",
        "tos:ListBuckets",
        "tos:GetBucketACL",
        "tos:GetBucketVersioning",
        "tos:GetBucketTagging",
        "tos:GetBucketLifecycle",
        "tos:GetBucketPolicy"
      ]
    },
    "update": {
      "permissions": [
        "tos:HeadBucket",
        "tos:PutBucketVersioning",
        "tos:PutBucketACL",
        "tos:PutBucketTagging",
        "tos:PutBucketLifecycle",
        "tos:PutBucketStorageClass",
        "tos:ListBuckets",
        "tos:GetBucketACL",
        "tos:GetBucketVersioning",
        "tos:GetBucketTagging",
        "tos:GetBucketLifecycle",
        "tos:PutBucketPolicy",
        "tos:GetBucketPolicy",
        "tos:DeleteBucketPolicy"
      ]
    }
  },
  "primaryIdentifier": [
    "/properties/Name"
  ],
  "properties": {
    "ACL": {
      "$ref": "#/definitions/ACL",
      "description": "桶的访问控制权限。",
      "type": "object"
    },
    "ACLGrant": {
      "$ref": "#/definitions/ACLGrant",
      "type": "object"
    },
    "AzRedundancy": {
      "description": "桶的可用区冗余类型。包括single-az：单可用区冗余，multi-az：多可用区冗余。",
      "enum": [
        "single-az",
        "multi-az"
      ],
      "type": "string"
    },
    "BucketType": {
      "description": "桶的类型。包括hns：获取所有分层桶列表，fns：获取所有扁平桶列表。",
      "enum": [
        "fns",
        "hns"
      ],
      "type": "string"
    },
    "CreationDate": {
      "description": "桶的创建时间。",
      "semantic": "createdTime",
      "type": "string"
    },
    "EnableVersionStatus": {
      "description": "存储桶的版本控制状态。Enabled：开启版本控制功能。Suspended：暂停版本控制功能。",
      "enum": [
        "Enabled",
        "Suspended"
      ],
      "type": "string"
    },
    "ExtranetEndpoint": {
      "description": "存储桶的 TOS 协议公网访问域名。",
      "type": "string"
    },
    "IntranetEndpoint": {
      "description": "存储桶的 TOS 协议私网访问域名",
      "type": "string"
    },
    "Lifecycle": {
      "description": "存储桶的声明周期。",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/LifecycleRule",
        "type": "object"
      },
      "type": "array",
      "uniqueItems": true
    },
    "Location": {
      "description": "桶所在区域。",
      "type": "string"
    },
    "Name": {
      "description": "桶名。",
      "maxLength": 63,
      "minLength": 3,
      "semantic": "resourceId",
      "type": "string"
    },
    "Policy": {
      "description": "JSON 格式的字符串，包含了桶策略的信息，但单个桶的所有桶策略 JSON 序列化后总大小不能超过 20KB。",
      "type": "string"
    },
    "ProjectName": {
      "description": "存储桶所属项目。",
      "semantic": "projectName",
      "type": "string"
    },
    "StorageClass": {
      "description": "桶的默认存储类型。包括STANDARD：标准存储。IA：低频访问存储。INTELLIGENT_TIERING：智能分层存储。ARCHIVE_FR：归档闪回存储。ARCHIVE：归档存储。COLD_ARCHIVE：冷归档存储。DEEP_COLD_ARCHIVE：深度冷归档存储。",
      "enum": [
        "STANDARD",
        "IA",
        "ARCHIVE_FR",
        "INTELLIGENT_TIERING",
        "COLD_ARCHIVE",
        "ARCHIVE",
        "DEEP_COLD_ARCHIVE"
      ],
      "type": "string"
    },
    "Tags": {
      "description": "存储桶的标签信息。",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/Tag",
        "type": "object"
      },
      "semantic": "tags",
      "type": "array",
      "uniqueItems": true
    }
  },
  "readOnlyProperties": [
    "/properties/Location",
    "/properties/CreationDate",
    "/properties/ExtranetEndpoint",
    "/properties/IntranetEndpoint"
  ],
  "required": [
    "Name"
  ],
  "tagging": {
    "tagOnCreate": true,
    "tagUpdatable": true,
    "taggable": true
  },
  "typeName": "Volcengine::TOS::Bucket"
}