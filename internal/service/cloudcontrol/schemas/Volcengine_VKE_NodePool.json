          "maximum": 100,
          "minimum": 1,
          "type": "integer"
        },
        "RegistryPullQps": {
          "description": "可用来限制镜像仓库的 QPS 上限",
          "maximum": 50,
          "minimum": 1,
          "type": "integer"
        },
