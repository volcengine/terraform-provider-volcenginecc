{
  "additionalProperties": false,
  "createOnlyProperties": [
    "/properties/FileSystemId"
  ],
  "description": "快照是一种备份方式，每个快照都是文件存储在某个时间点的备份。当数据丢失或故障时，您可以通过快照恢复文件存储数据，找回丢失的数据。",
  "filterProperties": {
    "filterableProperties": [
      "/properties/FileSystemId",
      "/properties/SnapshotId",
      "/properties/SnapshotName",
      "/properties/SnapshotType",
      "/properties/Status"
    ]
  },
  "handlers": {
    "create": {
      "permissions": [
        "FileNAS:CreateSnapshot",
        "FileNAS:DescribeSnapshots"
      ]
    },
    "delete": {
      "permissions": [
        "FileNAS:DeleteSnapshot",
        "FileNAS:DescribeSnapshots"
      ]
    },
    "list": {
      "permissions": [
        "FileNAS:DescribeSnapshots"
      ]
    },
    "read": {
      "permissions": [
        "FileNAS:DescribeSnapshots"
      ]
    },
    "update": {
      "permissions": [
        "FileNAS:UpdateSnapshot",
        "FileNAS:DescribeSnapshots"
      ]
    }
  },
  "primaryIdentifier": [
    "/properties/SnapshotId"
  ],
  "properties": {
    "CreatedTime": {
      "description": "快照创建时间。",
      "semantic": "createdTime",
      "type": "string"
    },
    "Description": {
      "description": "快照描述信息。",
      "semantic": "description",
      "type": "string"
    },
    "FileSystemId": {
      "description": "文件系统 ID。",
      "type": "string"
    },
    "FileSystemName": {
      "description": "文件系统名称。",
      "type": "string"
    },
    "IsEncrypt": {
      "description": "是否加密，当前仅支持false，即不加密。",
      "type": "boolean"
    },
    "Progress": {
      "description": "快照创建进度。",
      "type": "string"
    },
    "RetentionDays": {
      "default": 2147483647,
      "description": "快照保留时间，默认为2147483647，即永久保留。",
      "type": "integer"
    },
    "SnapshotId": {
      "description": "快照 ID。",
      "semantic": "resourceId",
      "type": "string"
    },
    "SnapshotName": {
      "description": "快照名称。",
      "semantic": "resourceName",
      "type": "string"
    },
    "SnapshotType": {
      "default": "Manual",
      "description": "快照类型，默认为Manual，即手动快照。",
      "enum": [
        "Manual"
      ],
      "type": "string"
    },
    "SourceSize": {
      "description": "文件系统容量，单位为 GiB。",
      "type": "integer"
    },
    "SourceVersion": {
      "description": "文件系统版本。",
      "type": "string"
    },
    "Status": {
      "description": "快照状态。说明如下：Progressing：正在创建。Accomplished：创建成功。Failed：创建失败。",
      "enum": [
        "Progressing",
        "Accomplished",
        "Failed"
      ],
      "semantic": "status",
      "type": "string"
    },
    "ZoneId": {
      "description": "可用区 ID。",
      "type": "string"
    }
  },
  "readOnlyProperties": [
    "/properties/CreatedTime",
    "/properties/FileSystemName",
    "/properties/Progress",
    "/properties/RetentionDays",
    "/properties/SnapshotId",
    "/properties/SnapshotType",
    "/properties/SourceSize",
    "/properties/SourceVersion",
    "/properties/Status",
    "/properties/ZoneId",
    "/properties/IsEncrypt"
  ],
  "required": [
    "FileSystemId",
    "SnapshotName"
  ],
  "tagging": {
    "taggable": false
  },
  "typeName": "Volcengine::FileNAS::Snapshot"
}