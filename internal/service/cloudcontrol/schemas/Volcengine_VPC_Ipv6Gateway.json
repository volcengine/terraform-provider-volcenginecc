{
  "additionalProperties": false,
  "createOnlyProperties": [
    "/properties/ProjectName",
    "/properties/VpcId"
  ],
  "definitions": {
    "Tag": {
      "description": "标签信息。",
      "properties": {
        "Key": {
          "description": "标签键。",
          "type": "string"
        },
        "Value": {
          "description": "标签值。",
          "type": "string"
        }
      },
      "required": [
        "Key"
      ],
      "type": "object"
    }
  },
  "description": "IPv6网关服务的运行载体，创建IPv6网关实例是进行IPv6公网配置的前提。",
  "filterProperties": {
    "filterableProperties": [
      "/properties/Ipv6GatewayId",
      "/properties/Name",
      "/properties/VpcId",
      "/properties/ProjectName",
      "/properties/Tags/*/Key",
      "/properties/Tags/*/Value"
    ]
  },
  "handlers": {
    "create": {
      "permissions": [
        "vpc:CreateIpv6Gateway",
        "vpc:DescribeIpv6GatewayAttribute",
        "vpc:TagResources"
      ]
    },
    "delete": {
      "permissions": [
        "vpc:DeleteIpv6Gateway",
        "vpc:DescribeIpv6GatewayAttribute"
      ]
    },
    "list": {
      "permissions": [
        "vpc:DescribeIpv6Gateways"
      ]
    },
    "read": {
      "permissions": [
        "vpc:DescribeIpv6GatewayAttribute"
      ]
    },
    "update": {
      "permissions": [
        "vpc:ModifyIpv6GatewayAttribute",
        "vpc:DescribeIpv6GatewayAttribute",
        "vpc:TagResources",
        "vpc:UntagResources"
      ]
    }
  },
  "primaryIdentifier": [
    "/properties/Ipv6GatewayId"
  ],
  "properties": {
    "CreatedTime": {
      "description": "IPv6网关实例的创建时间。",
      "semantic": "createdTime",
      "type": "string"
    },
    "Description": {
      "description": "IPv6网关实例的描述信息。长度限制为0~ 255个字符。不填默认为空字符串。需要以字母、中文或数字开头。可包含英文逗号（,）、点号（.）、下划线（_）、空格（ ）、等号（=）、短横线（-）、中文逗号（，）、中文句号（。）。",
      "semantic": "description",
      "type": "string"
    },
    "Ipv6GatewayId": {
      "description": "IPv6网关实例的ID。",
      "semantic": "resourceId",
      "type": "string"
    },
    "Name": {
      "description": "IPv6网关实例的名称。长度限制为1 ~ 128个字符。需要以字母、中文或数字开头，可包含点号（.）、下划线（_）和短横线（-）。不填默认为IPv6网关ID。",
      "semantic": "resourceName",
      "type": "string"
    },
    "ProjectName": {
      "description": "IPv6网关所属项目的名称。",
      "semantic": "projectName",
      "type": "string"
    },
    "RouteTableId": {
      "description": "IPv6网关实例关联的路由表ID。",
      "type": "string"
    },
    "Status": {
      "description": "IPv6网关实例的运行状态。取值如下：Creating：创建中。Deleting：删除中。Available：可用。",
      "semantic": "status",
      "type": "string"
    },
    "Tags": {
      "description": "标签信息",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/Tag"
      },
      "semantic": "tags",
      "type": "array",
      "uniqueItems": true
    },
    "UpdatedTime": {
      "description": "IPv6网关实例的修改时间。",
      "semantic": "updatedTime",
      "type": "string"
    },
    "VpcId": {
      "description": "需关联IPv6网关实例的VPC ID。每个VPC只能关联一个IPv6网关实例，需确保待关联的VPC未关联其他IPv6网关实例。",
      "type": "string"
    }
  },
  "readOnlyProperties": [
    "/properties/CreatedTime",
    "/properties/Ipv6GatewayId",
    "/properties/RouteTableId",
    "/properties/Status",
    "/properties/UpdatedTime"
  ],
  "required": [
    "VpcId"
  ],
  "tagging": {
    "tagOnCreate": true,
    "tagUpdatable": true,
    "taggable": true
  },
  "typeName": "Volcengine::VPC::Ipv6Gateway"
}