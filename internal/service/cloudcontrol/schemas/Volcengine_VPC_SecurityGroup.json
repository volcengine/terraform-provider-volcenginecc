    "/properties/IngressPermissions/*/UpdateTime",
    "/properties/EgressPermissions/*/CreationTime",
    "/properties/EgressPermissions/*/PrefixListCidrs",
    "/properties/EgressPermissions/*/UpdateTime"
  ],
  "required": [
    "VpcId"
  ],
  "tagging": {
    "tagOnCreate": true,
    "tagUpdatable": true,
    "taggable": true
  },
  "typeName": "Volcengine::VPC::SecurityGroup"
