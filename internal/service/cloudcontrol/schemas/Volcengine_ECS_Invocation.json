    "/properties/InvocationStatus",
    "/properties/CommandType",
    "/properties/CommandName",
    "/properties/ParameterDefinitions",
    "/properties/InvocationResults",
    "/properties/Instances"
  ],
  "required": [
    "CommandId",
    "InvocationName",
    "InstanceIds"
  ],
  "tagging": {
    "tagOnCreate": true,
    "tagUpdatable": true,
    "taggable": true
  },
  "typeName": "Volcengine::ECS::Invocation",
  "writeOnlyProperties": [
    "/properties/InstanceIds",
    "/properties/WindowsPassword"
  ]
