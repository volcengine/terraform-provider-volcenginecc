          "type": "integer"
        },
        "DefaultValue": {
          "description": "自定义参数默认值。",
          "type": "string"
        },
        "MaxLength": {
          "description": "自定义参数值（字符串）的最大长度。",
          "maximum": 1000,
          "type": "integer"
        },
        "MaxValue": {
          "description": "自定义参数值（数字）的最大值。",
