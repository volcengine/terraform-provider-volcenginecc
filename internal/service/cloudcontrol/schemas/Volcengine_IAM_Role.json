{
  "typeName": "Volcengine::IAM::Role",
  "description": "角色",
  "additionalProperties": false,
  "properties": {
    "RoleName": {
      "type": "string",
      "description": "角色名"
    },
    "DisplayName": {
      "type": "string",
      "description": "角色显示名"
    },
    "Description": {
      "type": "string",
      "description": "角色描述"
    },
    "TrustPolicyDocument": {
      "type": "string",
      "description": "角色信任策略"
    },
    "MaxSessionDuration": {
      "type": "integer",
      "description": "角色最大会话时间",
      "minimum": 3600,
      "maximum": 43200
    },
    "Tags": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Tag"
      }
    },
    "IsServiceLinkedRole": {
      "type": "integer",
      "description": "是否是服务关联角色, 0否，1是",
      "enum": [
        0,
        1
      ]
    },
    "Trn": {
      "type": "string",
      "description": "角色TRN"
    },
    "RoleId": {
      "type": "integer",
      "description": "角色ID"
    },
    "CreateDate": {
      "type": "string",
      "description": "角色创建时间"
    },
    "UpdateDate": {
      "type": "string",
      "description": "角色更新时间"
    },
    "Policies": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Policy"
      }
    }
  },
  "definitions": {
    "Tag": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "Key": {
          "type": "string"
        },
        "Value": {
          "type": "string"
        }
      },
      "required": [
        "Key",
        "Value"
      ]
    },
    "Policy": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "PolicyName": {
          "type": "string"
        },
        "PolicyType": {
          "type": "string",
          "enum": [
            "System",
            "Custom"
          ]
        }
      },
      "required": [
        "PolicyName",
        "PolicyType"
      ]
    }
  },
  "primaryIdentifier": [
    "/properties/RoleName"
  ],
  "readOnlyProperties": [
    "/properties/IsServiceLinkedRole",
    "/properties/Trn",
    "/properties/RoleId",
    "/properties/CreateDate",
    "/properties/UpdateDate"
  ],
  "required": [
    "RoleName"
  ],
  "tagging": {
    "taggable": false
  },
  "handlers": {
    "create": {
      "permissions": [
        "iam:CreateRole",
        "iam:AttachRolePolicy",
        "iam:GetRole",
        "iam:CreateServiceLinkedRole"
      ]
    },
    "read": {
      "permissions": [
        "iam:GetRole",
        "iam:CreateServiceLinkedRole"
      ]
    },
    "update": {
      "permissions": [
        "iam:UpdateRole",
        "iam:AttachRolePolicy",
        "iam:DetachRolePolicy",
        "iam:GetRole",
        "iam:CreateServiceLinkedRole"
      ]
    },
    "delete": {
      "permissions": [
        "iam:DeleteRole",
        "iam:GetRole"
      ]
    },
    "list": {
      "permissions": [
        "iam:ListRoles",
        "iam:GetRole",
        "iam:CreateServiceLinkedRole"
      ]
    }
  }
}