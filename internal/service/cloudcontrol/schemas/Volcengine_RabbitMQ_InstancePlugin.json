{
  "additionalProperties": false,
  "createOnlyProperties": [
    "/properties/InstanceId",
    "/properties/PluginName"
  ],
  "description": "在消息队列 RabbitMQ版控制台中创建实例之后，实例插件默认为关闭状态，您可以手动为实例开启/关闭插件。开启/关闭部分插件会导致集群滚动重启，建议避开业务运行时段执行该操作。集群滚动重启时会造成未持久化的消息丢失，也会出现短暂的连接不稳定现象，例如连接断开，断开后会根据客户端配置决定是否自动重连。",
  "filterProperties": {
    "filterableProperties": [
      "/properties/InstanceId"
    ],
    "required": [
      "/properties/InstanceId"
    ]
  },
  "handlers": {
    "create": {
      "permissions": [
        "RabbitMQ:DescribeInstanceDetail",
        "RabbitMQ:ModifyPlugin",
        "RabbitMQ:DescribePlugins"
      ]
    },
    "delete": {
      "permissions": [
        "RabbitMQ:DescribeInstanceDetail",
        "RabbitMQ:ModifyPlugin",
        "RabbitMQ:DescribePlugins"
      ]
    },
    "list": {
      "permissions": [
        "RabbitMQ:DescribePlugins"
      ]
    },
    "read": {
      "permissions": [
        "RabbitMQ:DescribePlugins"
      ]
    }
  },
  "primaryIdentifier": [
    "/properties/InstanceId",
    "/properties/PluginName"
  ],
  "properties": {
    "Description": {
      "description": "插件描述。",
      "semantic": "description",
      "type": "string"
    },
    "DisablePrompt": {
      "description": "关闭此插件的注意事项说明。",
      "type": "string"
    },
    "EnablePrompt": {
      "description": "开启此插件的注意事项说明。",
      "type": "string"
    },
    "Enabled": {
      "description": "是否已开启插件。true：插件已开启。false：插件已关闭。",
      "type": "boolean"
    },
    "InstanceId": {
      "description": "实例 ID。",
      "type": "string"
    },
    "NeedRebootOnChange": {
      "description": "开启或关闭插件是否会引发 RabbitMQ 实例重启。true：会重启实例。false：不会重启实例。",
      "type": "boolean"
    },
    "PluginName": {
      "description": "插件名称。",
      "semantic": "resourceId",
      "type": "string"
    },
    "Port": {
      "description": "插件监听的端口。",
      "type": "integer"
    },
    "Version": {
      "description": "插件版本。",
      "type": "string"
    }
  },
  "readOnlyProperties": [
    "/properties/Description",
    "/properties/DisablePrompt",
    "/properties/EnablePrompt",
    "/properties/NeedRebootOnChange",
    "/properties/Port",
    "/properties/Version",
    "/properties/Enabled"
  ],
  "required": [
    "InstanceId"
  ],
  "tagging": {
    "taggable": false
  },
  "typeName": "Volcengine::RabbitMQ::InstancePlugin"
}